script "JonTauntCooldown" (void) //458
{
    while (CheckInventory("JonTauntCooldown") > 0)
    {
        Delay(35);
        TakeInventory("JonTauntCooldown", 1);
    }
}

Script "JonBallGag" (int type) NET //461
{
	if(CheckInventory("JonBallgag"))
	{
		TakeInventory("JonBallgag", 1);
		Print(s:"Ballgag Off");
	}
	else
	{
		GiveInventory("JonBallGag", 1);
		Print(s:"Ballgag On");
	}
}
