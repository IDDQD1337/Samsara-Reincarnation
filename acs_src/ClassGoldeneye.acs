Script "Goldfinger_MoveDB5" (int interpolate)
{
	int startingviewHeight = ACS_NamedExecuteWithResult("DB5ACSCompanion",0,0,AAPTR_TARGET);
	int startingattackOffset = ACS_NamedExecuteWithResult("DB5ACSCompanion",1,0,AAPTR_TARGET);
	str startingspecies = ACS_NamedExecuteWithResult("DB5ACSCompanion",4,0,AAPTR_TARGET);
	int db5Seed = PlayerNumber();
	while(ACS_NamedExecuteWithResult("Goldeneye_IsUsingTank"))
	{
		ACS_NamedExecuteWithResult("Goldfinger_DB5MoveInput",interpolate);	
		Warp(AAPTR_TRACER,0,0,0,0,WARPF_NOCHECKPOSITION|WARPF_USEPTR|interpolate);
		Warp(AAPTR_TARGET,-10.0,14.0,-20.0+ACS_NamedExecuteWithResult("DB5ACSCompanion",18,0,AAPTR_TRACER),0,WARPF_NOCHECKPOSITION|WARPF_MOVEPTR|WARPF_USEPTR|interpolate);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",19,ACS_NamedExecuteWithResult("DB5ACSCompanion",16,0,AAPTR_TRACER),AAPTR_TARGET);
		ACS_NamedExecuteWithResult("DB5PlayerAngle",AAPTR_TRACER);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",2,53.0,AAPTR_TARGET);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",3,startingattackOffset+8.0,AAPTR_TARGET);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",5,strparam(s:"DB5",d:db5Seed),AAPTR_TARGET);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",5,strparam(s:"DB5",d:db5Seed),AAPTR_TRACER);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",7,GetActorAngle(0)%1.0,AAPTR_TARGET);
		Delay(1);
	}
	TakeInventory("Goldeneye_UsingTank",1);
	ACS_NamedExecuteWithResult("DB5ACSCompanion",6,0,AAPTR_TRACER);
	SetActivator(0,AAPTR_TARGET);
	ACS_NamedExecuteWithResult("DB5ACSCompanion",2,startingviewHeight);
	ACS_NamedExecuteWithResult("DB5ACSCompanion",3,startingattackOffset);
	ACS_NamedExecuteWithResult("DB5ACSCompanion",5,startingspecies);
	
	//Fix player position when exiting
	if(!CheckInventory("Goldfinger_DB5EjectorSeat"))
	{
		if(!Warp(0,0,32.0,0,0,WARPF_MOVEPTR))
			if(!Warp(0,0,-80.0,0,0,WARPF_MOVEPTR))
				if(!Warp(0,80.0,0,0,0,WARPF_MOVEPTR))
					if(!Warp(0,-64.0,0,0,0,WARPF_MOVEPTR))
						Warp(0,0,0,40.0,0,WARPF_MOVEPTR);	
	}
	else
	{
		ThrustThingZ(0,72,0,1);
		Warp(0,0,0,40.0,0,WARPF_MOVEPTR);
	}
	TakeInventory("Goldfinger_DB5EjectorSeat",1);
}

Script "Goldfinger_DB5MoveInput" (int interpolate)
{
	int myTid = UniqueTid();
	Thing_ChangeTid(0,myTid);
	SetActivator(0,AAPTR_Target);

	int playerForward = GetPlayerInput(-1,INPUT_FORWARDMOVE)/16;
	int playerSideMove = GetPlayerInput(-1,INPUT_SIDEMOVE);
	int cruiseControl = GetPlayerInput(-1,INPUT_BUTTONS) & BT_JUMP;
	int accelerate = GetPlayerInput(-1,INPUT_FORWARDMOVE) > 2048;

	if(playerForward == 0)
	{
		playerForward = CheckInventory("Goldeneye_TankForward")-65536;
		playerForward = FixedMul(playerForward,0.98);
		
		if(abs(playerForward) <= 0.0125) playerForward = 0;
		
		SetInventory("Goldeneye_TankForward",playerForward+65536);
	}
	else
	{
		if(playerForward > 0)
		{
			int speedRatio = (1.0 - ((1.0*(CheckInventory("Goldeneye_TankForward")-65536) / 28000)));
			if(speedRatio < 0) speedRatio = 0;
			if(CheckInventory("Goldeneye_TankForward")-65536 < 28000 && !cruiseControl)
				GiveInventory("Goldeneye_TankForward",(playerForward*speedRatio)>>16);	
		}
		else
		{
			if(CheckInventory("Goldeneye_TankForward")-65536 > -12000)
				TakeInventory("Goldeneye_TankForward",abs(playerForward/2));
		}
		playerForward = CheckInventory("Goldeneye_TankForward")-65536;
		SetInventory("Goldeneye_TankForward",playerForward+65536);
	}

	if(playerSideMove == 0)
	{
		playerSideMove = CheckInventory("Goldeneye_TankSide")-65536;
		playerSideMove = FixedMul(playerSideMove,0.667);
		if(abs(playerSideMove) <= 3) playerSideMove = 0;
		SetInventory("Goldeneye_TankSide",playerSideMove+65536);
	}
	else
	{
		SetInventory("Goldeneye_TankSide",playerSideMove+65536);
	}

	if(abs(playerForward) > 1024 || abs(playerSideMove) > 1024 != 0)
	{
		SetActivator(myTid,AAPTR_Tracer);
		int angle = GetActorAngle(0);
		SetActivator(myTid,AAPTR_Target);
		SetInventory("Goldeneye_TankBaseAngle",angle);
		GiveInventory("Goldfinger_DB5DamageObjects",1);
	}

	SetActivator(myTid,AAPTR_Tracer);
	if(CheckInventory("Goldeneye_Tank_Blocked"))
	{
		TakeInventory("Goldeneye_Tank_Blocked",1);
		SetActivator(myTid,AAPTR_TARGET);
		SetInventory("Goldeneye_TankForward",65536);
		playerForward = 0;
		SetActivator(myTid,AAPTR_Tracer);
	}
	ACS_NamedExecuteWithResult("Goldfinger_DB5Move",playerForward,-playerSideMove,accelerate,interpolate);
	Thing_ChangeTid(myTid,0);
}

Script "Goldfinger_DB5Move" (int playerForward, int playerSideMove, int accelerate, int interpolate)
{
	int angle = GetActorAngle(0);
	int x = GetActorX(0);
	int y = GetActorY(0);
	int z = GetActorZ(0);
	int forwardOffset = 64/* + playerForward/500*/;
	int backwardOffset = 96;
	int sideOffset = 16;
	int dummyTid = UniqueTid();
	if(playerForward > 0)
	{
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * cos(angle)),y + (forwardOffset * sin(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * cos(angle)) - (sideOffset * sin(angle)),y + (forwardOffset * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * cos(angle)) + (sideOffset * sin(angle)),y + (forwardOffset * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * cos(angle)) - (sideOffset * 2 * sin(angle)),y + (forwardOffset * sin(angle)) + (sideOffset * 2 * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * cos(angle)) + (sideOffset * 2 * sin(angle)),y + (forwardOffset * sin(angle)) - (sideOffset * 2 * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		SetActorAngle(dummyTid,angle);
	}
	if(playerForward < 0)
	{
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (backwardOffset * cos(angle)),y - (backwardOffset * sin(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (backwardOffset * cos(angle)) - (sideOffset * sin(angle)),y - (backwardOffset * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (backwardOffset * cos(angle)) + (sideOffset * sin(angle)),y - (backwardOffset * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (backwardOffset * cos(angle)) - (sideOffset * 2 * sin(angle)),y - (backwardOffset * sin(angle)) + (sideOffset * 2 * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (backwardOffset * cos(angle)) + (sideOffset * 2 * sin(angle)),y - (backwardOffset * sin(angle)) - (sideOffset * 2 * cos(angle)),z + 12.0,dummyTid)) playerForward = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		SetActorAngle(dummyTid,angle+0.5);
	}
	forwardOffset = 30;
	sideOffset = 36;
	if(playerSideMove > 0)
	{
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (sideOffset * sin(angle)),y + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * cos(angle)) - (sideOffset * sin(angle)),y + (forwardOffset * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * 2 * cos(angle)) - (sideOffset * sin(angle)),y + (forwardOffset * 2 * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * 3 * cos(angle)) - (sideOffset * sin(angle)),y + (forwardOffset * 3 * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (forwardOffset * cos(angle)) - (sideOffset * sin(angle)),y - (forwardOffset * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (forwardOffset * 2 * cos(angle)) - (sideOffset * sin(angle)),y - (forwardOffset * 2 * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (forwardOffset * 3 * cos(angle)) - (sideOffset * sin(angle)),y - (forwardOffset * 3 * sin(angle)) + (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		SetActorAngle(dummyTid,angle+0.25);
	}
	if(playerSideMove < 0)
	{
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (sideOffset * sin(angle)),y - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * cos(angle)) + (sideOffset * sin(angle)),y + (forwardOffset * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * 2 * cos(angle)) + (sideOffset * sin(angle)),y + (forwardOffset * 2 * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x + (forwardOffset * 3 * cos(angle)) + (sideOffset * sin(angle)),y + (forwardOffset * 3 * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (forwardOffset * cos(angle)) + (sideOffset * sin(angle)),y - (forwardOffset * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (forwardOffset * 2 * cos(angle)) + (sideOffset * sin(angle)),y - (forwardOffset * 2 * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		if(!Spawn("Goldfinger_DB5_CollisionBoundary",x - (forwardOffset * 3 * cos(angle)) + (sideOffset * sin(angle)),y - (forwardOffset * 3 * sin(angle)) - (sideOffset * cos(angle)),z + 12.0,dummyTid)) playerSideMove = 0;
		SetPointerExt(AAPTR_MASTER,0,dummyTid);
		SetActorAngle(dummyTid,angle-0.25);
	}
	
	Delay(1);
	SetActorAngle(0,angle+playerSideMove/16);
	if(accelerate) PlaySound(0,"Goldfinger/DB5/Move",CHAN_6,0.5,1,0.5);
	else { PlaySound(0,"Goldeneye/Tank/Idle",CHAN_5,0.25,1,0.5); StopSound(0,CHAN_6); }
	Thing_Remove(dummyTid);
	if(CheckInventory("Goldeneye_Tank_Blocked"))
	{
		playerForward = 0; 
		playerSideMove = 0;
		//TakeInventory("Goldeneye_Tank_Blocked",1);
	}
	else Warp(0,playerForward*60,0,0,0,WARPF_NOCHECKPOSITION|interpolate);
	
	//PrintBold(f:ACS_NamedExecuteWithResult("DB5ACSCompanion",18));
}

