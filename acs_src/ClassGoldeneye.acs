Script "Goldfinger_DB5_BuildSuspension" (void)
{
	int partTid = UniqueTid();
	int wheelFR, wheelFL, wheelBR, wheelBL;
	int vecF, vecB, vecR, vecL;
	int roll, pitch;
	
	SpawnForced("Goldfinger_DB5Suspension_WheelFR",0,0,0,partTid);
	SetPointerExt(AAPTR_Master,0,partTid);
	Thing_ChangeTid(partTid,0);
	
	SpawnForced("Goldfinger_DB5Suspension_WheelFL",0,0,0,partTid);
	SetPointerExt(AAPTR_Master,0,partTid);
	Thing_ChangeTid(partTid,0);
	
	SpawnForced("Goldfinger_DB5Suspension_WheelBR",0,0,0,partTid);
	SetPointerExt(AAPTR_Master,0,partTid);
	Thing_ChangeTid(partTid,0);
	
	SpawnForced("Goldfinger_DB5Suspension_WheelBL",0,0,0,partTid);
	SetPointerExt(AAPTR_Master,0,partTid);
	Thing_ChangeTid(partTid,0);
	
	while(ClassifyActor(0) > 1)
	{
		Warp(AAPTR_TRACER,0,0,0,0,WARPF_NOCHECKPOSITION|WARPF_USEPTR);
		wheelFR = (CheckInventory("Goldfinger_DB5_WheelFRZ")-65536)<<16;
		wheelFL = (CheckInventory("Goldfinger_DB5_WheelFLZ")-65536)<<16;
		wheelBR = (CheckInventory("Goldfinger_DB5_WheelBRZ")-65536)<<16;
		wheelBL = (CheckInventory("Goldfinger_DB5_WheelBLZ")-65536)<<16;
		
		vecF = (wheelFR + wheelFL)/2;
		vecB = (wheelBR + wheelBL)/2;
		vecR = (wheelFR + wheelBR)/2;
		vecL = (wheelFL + wheelBL)/2;
		pitch = VectorAngle(94.0,vecF - vecB);
		roll = VectorAngle(56.0,vecR - vecL);
		//PrintBold(s:"VecF: ",f:vecF);
		//PrintBold(s:"VecB: ",f:vecB);
		
		ACS_NamedExecuteWithResult("DB5ACSCompanion",14,-pitch,AAPTR_TRACER);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",15,-roll,AAPTR_TRACER);
		
		ACS_NamedExecuteWithResult("DB5ACSCompanion",8,CheckInventory("Goldfinger_DB5_WheelFRZ")-65536,AAPTR_TRACER);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",9,CheckInventory("Goldfinger_DB5_WheelFLZ")-65536,AAPTR_TRACER);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",10,CheckInventory("Goldfinger_DB5_WheelBRZ")-65536,AAPTR_TRACER);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",11,CheckInventory("Goldfinger_DB5_WheelBLZ")-65536,AAPTR_TRACER);
		
		Delay(1);
	}
}

Script "Goldfinger_DB5_WheelThink" (int wheel)
{
	int wheelX, wheelY, wheelZ, parentZ, diff;
	while(ClassifyActor(0) > 1)
	{
		parentZ = round(ACS_NamedExecuteWithResult("DB5ACSCompanion",12,0,AAPTR_MASTER));
		diff = GetActorFloorZ(0) - parentZ;
		
		if(abs(diff) <= 24.0)
			wheelZ = diff;
		else wheelZ = 0;
				
		switch(wheel)
		{
			case 1:
				wheelX = 48.0;
				wheelY = 28.0;
				break;
			case 2:
				wheelX = 48.0;
				wheelY = -28.0;	
				break;
			case 3:
				wheelX = -46.0;
				wheelY = 28.0;
				break;
			case 4:	
				wheelX = -46.0;
				wheelY = -28.0;
				break;
		}
		
		//PrintBold(s:"floorZ: ",f:GetActorFloorZ(0));
		//PrintBold(s:"parentZ: ",f:parentZ);
		//PrintBold(s:"Diff: ",d:diff>>16);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",20+wheel,GetActorZ(0) - GetActorFloorZ(0) <= 1.0,AAPTR_MASTER);
		Warp(AAPTR_MASTER,wheelX,wheelY,wheelZ,0,WARPF_NOCHECKPOSITION|WARPF_USEPTR);
		ACS_NamedExecuteWithResult("DB5ACSCompanion",7+wheel,wheelZ>>16,AAPTR_MASTER);
		
		Delay(1);
	}
}