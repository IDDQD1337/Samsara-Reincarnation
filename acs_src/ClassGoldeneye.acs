Script "Goldfinger_DB5Activator" (void)
{
	int tankStart = timer();
	int yawStorage = GetActorAngle(0);
	int GZDoom = (GetCvar("samsara_zscriptready") > 0);
	int interpolate = WARPF_INTERPOLATE * GZDoom;
	ACS_NamedExecuteWithResult("Goldfinger_MoveDB5", interpolate);
	SetActivator(0,AAPTR_Target);
	GiveInventory("Goldeneye_TankForward",65536);
	GiveInventory("Goldeneye_TankSide",65536);
	while(CheckInventory("Goldeneye_UsingTank"))
	{
		//GiveInventory("SamsaraGoldenEyeTankProtection",1);
		SetPlayerProperty(0,1,PROP_FROZEN);
		if ((GetPlayerInput(-1,INPUT_BUTTONS) & BT_USE && timer() > (tankStart + 35) && !CheckInventory("BondWatchToken")) || isDead(0)) { TakeInventory("Goldeneye_UsingTank",1); }
		int playerYaw = GetPlayerInput(-1,INPUT_YAW);	
		int playerPitch = GetPlayerInput(-1,INPUT_PITCH);
		yawStorage += playerYaw;
		if(yawStorage > 1.0) yawStorage -= 1.0;
		if(yawStorage < 0.0) yawStorage += 1.0;
		SetInventory("Goldeneye_TankAngle",yawStorage);

		SetActorVelocity(0,0,0,0,0,0);
		Delay(1);
	}
	//TakeInventory("PowerSamsaraGoldenEyeTankProtection",1);
	SetPlayerProperty(0,0,PROP_FROZEN);
	TakeInventory("Goldeneye_TankForward",131072);
	TakeInventory("Goldeneye_TankSide",131072);
}

//mode, view height, attack z offset
Script "DB5ACSCompanion" (int mode, int value, int who)
{
	if(who != 0) SetActivator(0,who);
	switch(mode)
	{
		case 0:
			SetResultValue(GetActorProperty(0,APROP_ViewHeight));
			break;
		case 1:
			SetResultValue(GetActorProperty(0,APROP_AttackZOffset));
			break;
		case 2:
			SetActorProperty(0,APROP_ViewHeight,value);
			break;
		case 3:
			SetActorProperty(0,APROP_AttackZOffset,value);
			break;
		case 4:
			SetResultValue(GetActorProperty(0,APROP_SPECIES));
			break;
		case 5:
			SetActorProperty(0,APROP_Species,value);
			break;
		case 6:
			TakeInventory("Goldeneye_UsingTank",1);
			StopSound(0,CHAN_5);
			StopSound(0,CHAN_6);
			break;
		case 7:
			SetInventory("Goldfinger_DB5CarAngle",value+65536);
			break;
		case 8: 
			SetInventory("Goldfinger_DB5_WheelFRZ",value+65536); 
			break;
		case 9: 
			SetInventory("Goldfinger_DB5_WheelFLZ",value+65536);
			break;
		case 10: 
			SetInventory("Goldfinger_DB5_WheelBRZ",value+65536); 
			break;
		case 11: 
			SetInventory("Goldfinger_DB5_WheelBLZ",value+65536); 
			break;
		case 12: SetResultValue(GetActorZ(0)); break;
		case 13:
			Warp(0,0,0,value,0,WARPF_NOCHECKPOSITION);
			break;
		case 14: ChangeActorPitch(0,value,true); break;
		case 15: ChangeActorRoll(0,value,true); break;
		case 16: SetResultValue(GetActorPitch(0)); break;
		case 17: SetResultValue(GetActorRoll(0)); break;
		case 18: 
			int wheelFR = CheckInventory("Goldfinger_DB5_WheelFRZ")-65536;
			int wheelFL = CheckInventory("Goldfinger_DB5_WheelFLZ")-65536;
			int wheelBR = CheckInventory("Goldfinger_DB5_WheelBRZ")-65536;
			int wheelBL = CheckInventory("Goldfinger_DB5_WheelBLZ")-65536;
			int minWheelZ = min(wheelFR,min(wheelFL,min(wheelBR,wheelBL)))<<16;
			int averageOffset = ((wheelFR+wheelFL+wheelBR+wheelBL)/4)<<16;
			//PrintBold(s:"Class 2: ",s:GetActorClass(0));
			//PrintBold(d:CheckInventory("Goldfinger_DB5_WheelFRZFloor")+CheckInventory("Goldfinger_DB5_WheelFLZFloor")+CheckInventory("Goldfinger_DB5_WheelBRZFloor")+CheckInventory("Goldfinger_DB5_WheelBLZFloor"));
			if(CheckInventory("Goldfinger_DB5_WheelFRZFloor")+CheckInventory("Goldfinger_DB5_WheelFLZFloor")+CheckInventory("Goldfinger_DB5_WheelBRZFloor")+CheckInventory("Goldfinger_DB5_WheelBLZFloor")==0)
			{
				minWheelZ = 0;	
				averageOffset = 0;
			}
			/*PrintBold(s:"wheelFR: ",d:wheelFR);
			PrintBold(s:"wheelFL: ",d:wheelFL);
			PrintBold(s:"wheelBR: ",d:wheelBR);
			PrintBold(s:"wheelBL: ",d:wheelBL);
			PrintBold(s:"minWheelZ: ",d:minWheelZ);
			PrintBold(s:"actorZ: ",f:GetActorZ(0));
			PrintBold(s:"floorZ: ",f:GetActorFloorZ(0));
			PrintBold(s:"averageOffset: ",f:averageOffset);*/
			
			SetResultValue(minWheelZ + abs(averageOffset)); 
			break;
		case 19: SetInventory("Goldfinger_DB5CarPitch",value+65536); break;
		case 20: SetResultValue(CheckInventory("Goldfinger_DB5CarPitch")-65536);  break;
		case 21: SetActivator(0,AAPTR_TRACER); SetInventory("Goldfinger_DB5_WheelFRZFloor",value); break;
		case 22: SetActivator(0,AAPTR_TRACER); SetInventory("Goldfinger_DB5_WheelFLZFloor",value); break;
		case 23: SetActivator(0,AAPTR_TRACER); SetInventory("Goldfinger_DB5_WheelBRZFloor",value); break;
		case 24: SetActivator(0,AAPTR_TRACER); SetInventory("Goldfinger_DB5_WheelBLZFloor",value); break;
	}
}

