actor ArmorScrap200 : ArmorScrap replaces ArmorScrap
{ 
	Armor.MaxSaveAmount 250 
	States
	{
	Spawn:
		TNT1 A 3
		TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(2689,2) == 0, 1)
		Goto SpawnCancel
        BON2 A 0 BRIGHT
        BON2 A -1 BRIGHT ACS_ExecuteAlways(212, 0, 4, 100, 90)
        stop
	SpawnCancel:
		TNT1 A 1 A_FadeOut
		stop

	}
}

actor GreenArmorPickup replaces GreenArmor
{
    +NOINTERACTION

    States
    {
      Spawn:
        RMR1 A 0 bright
	    TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(SAMSARA_DECORATE,29,0,0) == 1, "Armor5")
		TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(SAMSARA_DECORATE, 19), "Armor5")
        TNT1 A 35 ACS_ExecuteWithResult(SAMSARA_DECORATE, 18, 0)
        stop

      Armor1:
      Armor1_WithYellow:
        TNT1 A 0 A_SpawnItemEx("GreenArmorNoDrop",0,0,0,0,0,0,0,SXF_TRANSFERSPECIAL)
