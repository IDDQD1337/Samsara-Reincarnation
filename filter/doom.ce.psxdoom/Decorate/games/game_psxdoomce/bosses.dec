actor PsxBossEye_Samsara : BossEye replaces BossEye2
{
    DropItem "PsxDoomImp_Samsara", 255, 50
    DropItem "PsxDemon_Samsara", 255, 40
    DropItem "PsxSpectre_Samsara", 255, 20
    DropItem "PsxNightmareSpectre_Samsara", 255, 20
    DropItem "PsxCacodemon_Samsara", 255, 30
    DropItem "PsxHellKnight_Samsara", 255, 40
    Tag "Icon of Sin"
    +NOTIMEFREEZE
    // no need for the other flags since BossBrain is the thing you kill
    States
    {
      Spawn:
        SSWV A 0
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraBossMonologue", 5)
        goto Idle

      SpawnMonologue:
        SSWV A 80 A_SetTranslucent(0)
        TNT1 A 0 A_SetTranslucent(0.1)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.2)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.3)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.4)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.5)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.6)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.7)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.8)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.9)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(1)
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraBossMonologueSetRenderStyle", 1) // STYLE_Normal
        SSWV A 140 // for extra dialogue
        SSWV A 100
        goto Idle

      Idle:
        TNT1 A 0 A_GiveInventory("SamsaraBossMonologueFlagsDisablerBossEye")
      IdleLoop:
        SSWV A 10 A_Look
        loop

      See:
        TNT1 A 0 A_GiveInventory("SamsaraBossMonologueFlagsDisablerBossEye")
      See2:
        SSWV A 181 A_BrainAwake
        SSWV A 180 A_BrainSpit
        wait
    }
}

