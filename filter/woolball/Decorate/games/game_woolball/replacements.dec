Actor SamsaraClip2_ROTWB2 : Clip2
{
    States
    {
      Spawn:
        CLIP C 0
        goto SpawnGameModeCheck

      SpawnActual:
        TNT1 A 0 A_ChangeFlag("THRUACTORS",false)
        CLIP C -1 A_ChangeFlag("NOTIMEFREEZE",false)
        stop
    }
}

