actor D64Resurrector_Samsara : SamsaraMonsterDemonicBoss replaces D64Resurrector
{
	Health 5000
	Radius 60
	Height 150
	Mass 1000
	Speed 30
	PainChance 50
	ReactionTime 8
	Monster
	MinMissileChance 160
	+BOSS
	+MISSILEMORE
	+MISSILEEVENMORE
	+NOFEAR
	+FLOORCLIP
	+NORADIUSDMG
	+DONTMORPH
	+BOSSDEATH
	+DONTRIP
	SeeSound "mother/sight"
	AttackSound "mother/attack"
	PainSound ""
	DeathSound "mother/death"
	ActiveSound "mother/active"
	Obituary "$OB_MOTH"
	Tag "Mother Demon"
	Species "SamsaraResurrector"
	States
	{
	Spawn:
		RECT ABCD 8 A_Look
		Loop
	See:
		TNT1 A 0 A_GiveInventory("SamsaraBossSightTaunt")
	See2: // TODO: Convert A_RectChase to Decorate
		RECT AAABBBCCCDDD 3 A_Chase
		Loop
	Missile:
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnProjectile("D64RectFire", 0, 0, 0, CMF_AIMDIRECTION|CMF_BADPITCH, 0)
		TNT1 A 0 A_SpawnProjectile("D64RectFire", 0, 0, -45, CMF_AIMDIRECTION|CMF_BADPITCH, 0)
		TNT1 A 0 A_SpawnProjectile("D64RectFire", 0, 0, 45, CMF_AIMDIRECTION|CMF_BADPITCH, 0)
		TNT1 A 0 A_PlaySound("mother/attack", CHAN_BODY)
		RECT G 12 Bright
		RECT E 12
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnProjectile("D64RectBall", 68, 67, -90, CMF_AIMDIRECTION|CMF_BADPITCH)
		TNT1 A 0 A_SpawnProjectile("D64RectBall", 139, 49, -90, CMF_AIMDIRECTION|CMF_BADPITCH)
		TNT1 A 0 A_SpawnProjectile("D64RectBall", 68, -67, 90, CMF_AIMDIRECTION|CMF_BADPITCH)
		TNT1 A 0 A_SpawnProjectile("D64RectBall", 139, -49, 90, CMF_AIMDIRECTION|CMF_BADPITCH)
		RECT F 12 Bright
		RECT G 8
		Goto See2
	Pain:
		RECT E 8 A_PlaySound("mother/pain", CHAN_BODY, 1.0, false, 2.0)
		RECT H 18 A_Pain
		Goto See2
	Death: // TODO: Port over boss explosions
		TNT1 A 0 ACS_NamedExecuteAlways("Samsara_KillCount", 0, 0)
		RECT I 1
		RECT I 59 A_Scream
		RECT J 8
		TNT1 A 0 A_SpawnItemEx("UniversalBloodyHellGoreXDeathEffectForBosses",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_USEBLOODCOLOR)
		RECT K 8
		RECT L 5
		RECT M 4
		RECT N 3 A_NoBlocking
		RECT O 2
		TNT1 A -1 ACS_NamedExecuteWithResult("SamsaraBossDeathToggler")
		Stop
	}
}

actor D64BossEye_Samsara : BossEye replaces BossEye2
{
    DropItem "DoomImp", 255, 50
    DropItem "Demon", 255, 40
    DropItem "Spectre", 255, 20
    DropItem "Cacodemon", 255, 20
    DropItem "D64NightmareImp", 255, 30
    DropItem "HellKnight", 255, 40
    Tag "Icon of Sin"
    +NOTIMEFREEZE
    // no need for the other flags since BossBrain is the thing you kill
    States
    {
      Spawn:
        SSWV A 0
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraBossMonologue", 5)
        goto Idle

      SpawnMonologue:
        SSWV A 80 A_SetTranslucent(0)
