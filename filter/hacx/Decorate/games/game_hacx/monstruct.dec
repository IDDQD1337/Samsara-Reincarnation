			TNT1 A 0 A_ChangeFlag("NOPAIN",0)
			CPOS E 10 bright A_FaceTarget
			CPOS E 10 bright A_FatAttack1 ("HacxMonstructShot_Samsara")
			CPOS F 10 bright A_CPosAttack
			CPOS F 10 bright A_FatAttack2 ("HacxMonstructShot_Samsara")
			SSWV C 10 A_FaceTarget
			SSWV D 4 bright A_PosAttack
			SSWV C 4 A_PosAttack
			SSWV D 1 A_CPosRefire
			goto Missile+5
		Pain:
			CPOS D 5 A_Chase
			goto See
		Pain.MightyBoot:
			TNT1 A 0 A_ChangeFlag("NOPAIN",1)
			TNT1 A 0 A_PlaySoundEx("duke/kickhit","body",0,0)
			CPOS D 2 A_FaceTarget
			TNT1 A 0 A_Recoil(14)
			CPOS D 16 A_Pain
			Goto See        
		Death:
			TNT1 A 0 ACS_NamedExecuteAlways("Samsara_KillCount", 0, 0)
