actor SamsaraSlotThreeDrop2: SamsaraSlotThreePickup2 replaces SamsaraSlotThreeDrop
{
  States
  {
    Spawn:
      TNT1 A 0
      goto SpawnLoop
    Pickup:
      TNT1 A 0 A_JumpIfInventory("HalfLifeClass", 1, "CheckHL")	
	  TNT1 A 0 A_JumpIfInventory("StrifeClass", 1, "CheckStrife")	  
      TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3, 1) & 1) == 1, "PickupSuccess")
      goto PickupFailure
	CheckHL:
		TNT1 A 0 A_JumpIfInventory("Assault Shotgun", 1, "HLPickupAmmo")
        TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3, 1) & 1) == 1, 1)
		Goto PickupFailure
		TNT1 A 0 ACS_ExecuteAlways(2685,0,13)
		Goto PickupSuccess
	HLPickupAmmo:
		TNT1 A 0 A_GiveInventory("Shell",20)
		Stop
	CheckStrife:
		TNT1 A 0 A_JumpIfInventory("StrifeSigilPieceCheck2", 1, "StrifeCheck3")
		TNT1 A 0 A_GiveInventory("StrifeSigilPieceCheck2",1)	  
		TNT1 A 0 A_GiveInventory("StrifeSigilPiece",1)
	StrifeCheck3:	
        TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3, 1) & 1) == 1, "PickupSuccess")
        goto PickupFailure
  }
}

