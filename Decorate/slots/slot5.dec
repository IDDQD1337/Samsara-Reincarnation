actor SamsaraSlotFiveNoStay : SamsaraSlotFivePickup
{
    States
    {
      Spawn:
        WEPG E 0 Bright
        goto SpawnGameModeCheck

      SpawnActual:
        TNT1 A 0 A_ChangeFlag("THRUACTORS",false)
        TNT1 A 0 A_ChangeFlag("NOTIMEFREEZE",false)
        goto SpawnLoop

      PickupDefault:
        TNT1 A 0 A_JumpIf((CallACS("SamsaraGiveWeapon", 6, 1) & 1) == 1, "PickupSuccess")
        goto PickupFailure

      PickupStrife:
        TNT1 A 0 A_JumpIfInventory("StrifeGuyInStrife",1,"PickupDefault")
        TNT1 A 0 A_JumpIfInventory("GotWeapon5",1,"PickupDefault")
        TNT1 A 0 A_GiveInventory("StrifeSigilPiece")
        TNT1 A 0 A_GiveInventory(" Sigil ")
        TNT1 A 0 A_TakeInventory("StrifeSigilPiecePreferred")
        TNT1 A 0 A_GiveInventory("StrifeSigilPiecePreferred",CallACS("StrifeSigilPreferred"))
        goto PickupDefault
    }
}

actor SamsaraSlotFiveDrop : SamsaraSlotFiveNoStay
{
    States
    {
      PickupDefault:
        TNT1 A 0 A_JumpIf((CallACS("SamsaraGiveWeapon", 6, 2) & 1) == 1, "PickupSuccess")
        goto PickupFailure

      PickupStrife:
        TNT1 A 0 A_JumpIfInventory("StrifeGuyInStrife",1,"PickupDefault")
        TNT1 A 0 A_JumpIfInventory("GotWeapon5",1,"PickupDefault")
        TNT1 A 0 A_GiveInventory("StrifeSigilPiece")
        TNT1 A 0 A_GiveInventory(" Sigil ")
        TNT1 A 0 A_TakeInventory("StrifeSigilPiecePreferred")
        TNT1 A 0 A_GiveInventory("StrifeSigilPiecePreferred",CallACS("StrifeSigilPreferred"))
        goto PickupDefault
    }
}

