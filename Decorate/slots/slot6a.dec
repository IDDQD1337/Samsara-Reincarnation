actor SamsaraSlotSixDrop2: SamsaraSlotSixPickup2 replaces SamsaraSlotSixDrop
{
  States
  {
    Spawn:
      TNT1 A 0
      goto SpawnLoop
    Pickup:
	  TNT1 A 0 A_JumpIfInventory("HalfLifeClass", 1, "CheckHL")
	  TNT1 A 0 A_JumpIfInventory("StrifeClass", 1, "CheckStrife")	 
      TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 6, 1) & 1) == 1, "PickupSuccess")
      goto PickupFailure
	CheckHL:
	  TNT1 A 0 A_JumpIfInventory("Gauss Cannon", 1, "HLPickupAmmo")
      TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 6, 1) & 1) == 1, 1)
	  Goto PickupFailure
      TNT1 A 0 ACS_ExecuteAlways(2685,0,16)
	  Goto PickupSuccess
	HLPickupAmmo:
	  TNT1 A 0 A_GiveInventory("Cell",30)
	  TNT1 A 0 A_PlaySound("halflife/weaponpickup")
	  Stop
	CheckStrife:
	  TNT1 A 0 A_JumpIfInventory("GotWeapon6", 1, "StrifeCheck")
	  TNT1 A 0 A_GiveInventory("StrifeSigilPiece",1)
	  TNT1 A 0 A_GiveInventory(" Sigil ", 1)
	StrifeCheck:	
      TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 6) & 3) == 1, "PickupSuccess")
      goto PickupFailure
  }
}

