actor SamsaraSlotThreePickupF: SamsaraSlotThreePickup
{
  -DROPPED
  States
  {
    Spawn:
      WEPG C 0
      goto SpawnLoop
  }
}

actor SamsaraSlotThreeDrop: SamsaraSlotThreePickup
{
  States
  {
    Spawn:
      TNT1 A 0
      goto SpawnLoop
    Pickup:
      TNT1 A 0 A_JumpIfInventory("HalfLifeClass", 1, "CheckHL")	
	  TNT1 A 0 A_JumpIfInventory("StrifeClass", 1, "CheckStrife")	  
      TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3, 1) & 1) == 1, "PickupSuccess")
      goto PickupFailure
	CheckStrife:
	  TNT1 A 0 A_JumpIfInventory("GotWeapon3", 1, "StrifeCheck")
	  TNT1 A 0 A_GiveInventory("StrifeSigilPiece",1)
	  TNT1 A 0 A_GiveInventory(" Sigil ", 1)
	StrifeCheck:	
      TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3) & 3) == 1, "PickupSuccess")
      goto PickupFailure
	CheckHL:
	  TNT1 A 0 A_JumpIfInventory("Assault Shotgun", 1, "HLPickupAmmo")
      TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3, 1) & 1) == 1, 1)
	  Goto PickupFailure
	  TNT1 A 0 ACS_ExecuteAlways(2685,0,13)
	  Goto PickupSuccess
	HLPickupAmmo:
	  TNT1 A 0 A_JumpIfInventory("Shell",0,"PickupFailure")
	  TNT1 A 0 A_GiveInventory("Shell",8)
	  TNT1 A 0 A_PlaySound("halflife/weaponpickup")
	  Stop
	CheckMarathon:
	  TNT1 A 0 A_JumpIfInventory("Fusion Pistol", 1, "MarathonPickupAmmo")
	  TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3) & 3) == 1, "PickupSuccess")
	  Goto PickupFailure
	MarathonPickupAmmo:
	  TNT1 A 0 A_JumpIfInventory("Cell", 0, "PickupFailure")
	  TNT1 A 0 A_SetBlend("Green", 0.3, 3)
	  TNT1 A 0 A_PlaySound("marathon/itemget")
	  TNT1 A 0 A_GiveInventory("Cell", 20)
	  Stop	  

	
  }
}

