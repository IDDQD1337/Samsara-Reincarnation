actor SamsaraSlotThree_Strife:  SamsaraSlotThreePickup replaces MiniMissileLauncher
{
	States
	{
		CheckStrife:
			TNT1 A 0 A_JumpIfInventory("GotWeapon3", 1, "StrifeCheck")
		StrifeCheck:	
			TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 3) & 3) == 1, "PickupSuccess")
			goto PickupFailure
	}
}
