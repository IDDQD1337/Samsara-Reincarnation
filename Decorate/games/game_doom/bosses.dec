actor BossEye2 : BossEye replaces BossEye
{
    Tag "Icon of Sin"
    +NOTIMEFREEZE
    // no need for the other flags since BossBrain is the thing you kill
    States
    {
      Spawn:
        SSWV A 0
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraBossMonologue", 5)
        goto Idle

      SpawnMonologue:
        SSWV A 80 A_SetTranslucent(0)
        TNT1 A 0 A_SetTranslucent(0.1)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.2)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.3)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.4)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.5)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.6)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.7)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.8)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(0.9)
        SSWV AAAAAAAAAAA 14
        TNT1 A 0 A_SetTranslucent(1)
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraBossMonologueSetRenderStyle", 1) // STYLE_Normal
        SSWV A 140 // for extra dialogue
        SSWV A 100
        goto Idle

      Idle:
        TNT1 A 0 A_GiveInventory("SamsaraBossMonologueFlagsDisablerBossEye")
      IdleLoop:
        SSWV A 10 A_Look // Why the hell does the BossEye use Wolfenstein sprites?! // [tv50] normally doesn't show up due to +NOSECTOR (also explains why no StarSparkles used), but I'd like to know this, too
        loop

      See:
        TNT1 A 0 A_GiveInventory("SamsaraBossMonologueFlagsDisablerBossEye")
      See2:
        SSWV A 181 A_BrainAwake
        SSWV A 150 A_BrainSpit
        wait
    }
}

actor SamsaraBossMonologueFlagsDisablerBossEye : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraBossMonologueFlagsDisabled",1,"PickupStop")
        TNT1 A 0 A_ChangeFlag("NOTIMEFREEZE",false)
        TNT1 A 0 A_GiveInventory("SamsaraBossMonologueFlagsDisabled")
        stop
    }
}

