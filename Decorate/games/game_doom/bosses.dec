actor SpiderMastermind2 : SamsaraMonsterCyberneticBoss replaces SpiderMastermind
{
	Game Doom
	SpawnID 7
	Health 3000
	Radius 128
	Height 100
	Mass 1000
	Speed 12
	PainChance 40
	Monster
	MinMissileChance 160
	+BOSS
	+MISSILEMORE
	+FLOORCLIP
	+NORADIUSDMG
	+DONTMORPH
	+BOSSDEATH
	+DONTRIP
