		"####" "#" 1 A_SetScale(0.8*(user_Scalex/65536.0),0.8*(user_Scaley/65536.0))
		"####" "#" 0 A_SpawnItemEx("SuperCycloptis_2_ShrinkEffect",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
