		"####" "#" 1 A_SetScale(0.4*(user_Scalex/65536.0),0.4*(user_Scaley/65536.0))
		"####" "#" 0 A_SpawnItemEx("ArmoredFlemoidusBipedicusV3_2_ShrinkEffect",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
