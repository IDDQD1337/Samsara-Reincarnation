        TNT1 A 0 A_JumpIfInventory("Goldfinger_SmithWesson36",1,"PickupGoldfingerLoadout2Dual")
      PickupGoldfingerLoadout2Single:
        TNT1 A 0 A_GiveInventory("Goldfinger_SmithWesson36")
        TNT1 A 0 A_GiveInventory("Clip",18)
        TNT1 A 0 A_GiveInventory("Clip",10) // bonus ammo
        TNT1 A 0 A_JumpIf(CallACS("SamsaraDecorate", 65) == 3, 1)
        goto PickupGiveLimit
        TNT1 A 0 A_GiveInventory("Goldeneye_D5KDualToken")
        TNT1 A 0 A_GiveInventory("Clip",10)
        TNT1 A 0 A_GiveInventory("Clip",10) // bonus ammo
        goto PickupGiveLimit

      PickupGoldfingerLoadout2Dual:
        TNT1 A 0 A_GiveInventory("Goldeneye_D5KDualToken")
        TNT1 A 0 A_GiveInventory("Clip",10)
        TNT1 A 0 A_GiveInventory("Clip",10) // bonus ammo
        goto PickupGiveLimit

      PickupGoldfingerLoadout2Fill:
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraStrifeAccuracyAmmoFiller", 0)
        stop

      PickupGiveLimit:
        TNT1 A 0 A_JumpIf(CallACS("SamsaraDecorate", 65) <= 0, 1)
        stop
        TNT1 A 0 A_GiveInventory("Goldeneye_D5KLevelLimiter")
        stop
    }
}

