actor StrifeStaminaAccuracyReplacement : Trigger // hack to make the strifeguy stamina message show first
{
    +QUIET
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("StrifeClass",1,"PickupStaminaStrife")
      PickupStamina:
        TNT1 A 0 A_GiveInventory("UpgradeStamina",10)
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraStrifeStatsAdjust", 1)
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraAdjustHealthOverMaxDisplay")
        goto PickupAccuracy

      PickupStaminaStrife:
        TNT1 A 0 A_GiveInventory("StaminaUpgradeDamage",10)
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraPlayerSpawnHealthAdjust", 10, 1, 7, 100)
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraPlayerHealer")
        goto PickupAccuracy

      PickupAccuracy:
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraStrifeAccuracy", random(0,2))
        goto PickupFinish

      PickupFinish:
        TNT1 A 0 A_JumpIfInventory("StrifeClass",1,1)
        stop
        TNT1 A 0 ACS_NamedExecuteAlways("SamsaraClientMiscPickup", 0, 24, -1, 0)
        stop
    }
}

