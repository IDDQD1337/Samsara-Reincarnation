actor DoomStrifeUnique1 : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"PickupMessageInitStronghold")
        TNT1 A 0 A_JumpIfInventory("DoomClassMode",1,"PickupMessageInitDoom64")
        goto PickupMessageInitDone

      PickupMessageInitDoom64:
        TNT1 A 0 A_JumpIfInventory("SamsaraDoom64UnmakerArtifact",0,"PickupMessageInitDone")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoom64UnmakerArtifact",2,"PickupMessageInitDoom64_Artifact3")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoom64UnmakerArtifact",1,"PickupMessageInitDoom64_Artifact2")
      PickupMessageInitDoom64_Artifact1:
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraChoice1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraLoadout1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraDropped1")
        TNT1 A 0 A_GiveInventory("SamsaraMiscPickupDisplayExtraChoice1",2)
        goto PickupMessageInitDone

      PickupMessageInitDoom64_Artifact2:
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraChoice1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraLoadout1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraDropped1")
        TNT1 A 0 A_GiveInventory("SamsaraMiscPickupDisplayExtraChoice1",3)
        goto PickupMessageInitDone

      PickupMessageInitDoom64_Artifact3:
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraChoice1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraLoadout1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraDropped1")
        TNT1 A 0 A_GiveInventory("SamsaraMiscPickupDisplayExtraChoice1",4)
        goto PickupMessageInitDone

      PickupMessageInitStronghold:
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraChoice1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraLoadout1")
        TNT1 A 0 A_TakeInventory("SamsaraMiscPickupDisplayExtraDropped1")
        TNT1 A 0 A_GiveInventory("SamsaraMiscPickupDisplayExtraChoice1",10)
        goto PickupMessageInitDone

      PickupMessageInitDone:
        TNT1 A 0 A_Jump(256,"PickupStart") // for inheriting actors
        goto PickupStart

      PickupStart:
        TNT1 A 0 A_GiveInventory("DoomZerkCounter")
        TNT1 A 0 A_GiveInventory("SamsaraDoom64UnmakerArtifact")
        TNT1 A 0 A_JumpIfInventory("DoomClassMode",2,"PickupStronghold")
        TNT1 A 0 A_Jump(256,"PickupNotStronghold") // for inheriting actors
      PickupNotStronghold:
        TNT1 A 0 A_JumpIfInventory("DGStrHasHomingRocketLauncher",1,"PickupStop")
        TNT1 A 0 A_GiveInventory("DGStrHasHomingRocketLauncher")
        stop

      PickupStronghold:
        TNT1 A 0 A_JumpIfInventory("DGStrHasHomingRocketLauncher",1,"PickupStrongholdFill")
        TNT1 A 0 A_GiveInventory("Homing Rocket Launcher")
        TNT1 A 0 A_GiveInventory("RocketAmmo",4)
        TNT1 A 0 A_GiveInventory("DGStrHasHomingRocketLauncher")
        stop

      PickupStrongholdFill:
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraStrifeAccuracyAmmoFiller", 2)
        stop
    }
}

