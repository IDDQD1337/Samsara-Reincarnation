actor DMPhosFire : PhosphorousFire
{
	ReactionTime 17
	Radius 1
	Height 1
	Obituary "%k \cimade\c- %o \citake a flame bath."
	DamageType "PhosFire"
	+BRIGHT
	+FORCERADIUSDMG
	-EXTREMEDEATH
	+NOEXTREMEDEATH
	+MOVEWITHSECTOR
	states
	{
	Spawn:
		SBN2 B 2 A_Explode (20, 96)
		SBN2 C 2 A_Countdown
		SGFW A 0 A_PlaySound("strife/largefire", CHAN_VOICE)
		SGFW A 2 A_SpawnItemEx("DMPhosFire2", random(-7,7), random(-7,7), 0, random(-3,3), random(-3,3), 0, random(-180, 180))
		SGFW B 2 A_Countdown
		SGFW C 2 A_Explode (20, 96)
		SGFW D 3 A_Countdown
		SGFW E 3 A_Explode (20, 96)
		SGFW F 0 A_SetAngle(angle + random(15, 90))
		SGFW F 0 A_Recoil(random(-6, 6))
		SGFW F 3 A_Countdown
		SGFW G 0 A_PlaySound("strife/largefire", CHAN_VOICE)
		SGFW G 3 A_SpawnItemEx("DMPhosFire2", random(-7,7), random(-7,7), 0, random(-3,3), random(-3,3), 0, random(-180, 180))
		Goto Spawn+6
	Death:
		SGFW H 2
		SGFW I 0 A_PlaySound("strife/largefire", CHAN_VOICE)
		SGFW I 2 A_SpawnItemEx("DMPhosFire2", random(-7,7), random(-7,7), 0, random(-3,3), random(-3,3), 0, random(-180, 180))
		SGFW JK 2
		Stop
	}
}

