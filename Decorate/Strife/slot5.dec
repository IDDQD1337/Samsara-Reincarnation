actor StrifeHEGrenade
{
	Speed 15
	Radius 13
	Height 13
	Mass 20
	Damage (random(1,4)*1)
	ReactionTime 30
	Projectile
    -BLOODSPLATTER
	-NOGRAVITY
	+BOUNCEONACTORS
	+EXPLODEONWATER
	+FORCEXYBILLBOARD
	+WINDTHRUST
	+FORCERADIUSDMG
	MaxStepHeight 4
	BounceType "Doom"
	BounceFactor 0.5
	BounceCount 2
	DamageType "StrifeExplosive"
	SeeSound "strife/grenadeshoot"
	DeathSound "strife/grenadebang"
	Obituary "%k \ciinverted\c- %o \ciwith a H-E grenade."
	States
	{
	Spawn:
		HEGR AB 3 A_Countdown
		Loop
	Death:
		SBNG A 0 Bright A_Explode(192, 192, 1, 1)
		Goto DeathEnd
	DeathEnd:
		SBNG A 0 Bright A_SetTranslucent(1, 1)
		SBNG A 2 Bright A_NoGravity
		SBNG BCDEFGHIJKLMN 3 Bright
		Stop
	}
}

actor StrifeHEGrenade2 : StrifeHEGrenade { +THRUSPECIES Species "Player" +DONTHARMCLASS +DONTHARMSPECIES +MTHRUSPECIES }
actor RebelHEGrenade_2 : StrifeHEGrenade2
{
	DamageType "RebelMissile"
	States
	{
	Death:
		SBNG A 0 Bright A_Explode(80, 192, 1, 1)
		Goto Super::DeathEnd
	}
}
Actor RebelHEGrenade : StrifeHEGrenade
{
	States
	{
	Death:
		SBNG A 0 Bright A_Explode(80, 192, 0, 1)
		Goto Super::DeathEnd
	}
}

