actor " Grenade Launcher " : StrifeGrenadeLauncher
{
  Weapon.Kickback 100
  +INVENTORY.UNDROPPABLE
  Inventory.PickUpSound "strife/weaponget"
  Weapon.SelectionOrder 2400
  Weapon.AmmoUse1 0
  Weapon.AmmoUse2 0
  Weapon.AmmoGive1 16
  Weapon.AmmoGive2 0
  Weapon.AmmoType1 "RocketAmmo"
  Weapon.AmmoType2 "WhitePhosGrenade"
  Weapon.SlotNumber 5
  Weapon.SisterWeapon ""
