Actor UnrealBrassShell 
{
    Speed 2
    Mass 8
    Radius 2
    Height 2
	+MISSILE
    +CLIENTSIDEONLY
    +THRUACTORS
	+DROPOFF
	+NOTELEPORT
	+FLOORCLIP
	-SOLID
    States
    {
    Spawn:
		UN00 A 0
		UN00 A 0 A_SetPitch(pitch-15)
		UN00 A 1 A_SetAngle(angle-15)
        loop
    Death:
		UN00 A 0 A_JumpIf(waterlevel != 0,"Done")
		UN00 A 0 A_PlaySound("Unreal/Automag/Casing",CHAN_BODY)
		UN00 A 0 ThrustThingZ(0,4,0,0)
		goto Done
	Done:
		UN00 A 500
		UN00 A 1 A_FadeOut
		Wait
	}
}	

