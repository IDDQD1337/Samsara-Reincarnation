actor AlienSpectre1B : SamsaraMonsterReincAlienSpectre replaces AlienSpectre1
{
	DamageFactor "CorvusCrossbow", 1.15
	DamageFactor "DragonClaw", 1.5
	DamageFactor "PoisonBolt", 0
	DamageFactor "CorvusHellstaffTomed", 0.2
	DamageFactor "DeusExPoisonEffect", 0
	DamageFactor "DeusExKnockedOut", 0
	PainChance "MightyBoot", 0
	PainChance "PhosFire", 0
	PainChance "PhosFireCoop", 0
	PainChance "EradCoopGreenFire", 0
	PainChance "EradGreenFire", 0
	PainChance "StrifeExplosive", 0
	PainChance "PSFire", 5
	PainChance "CalebFire", 0
	PainChance "WTMelee", 96
	PainChance "WTMeleeIce", 96
	PainChance "ShrinkerDMG", 0
	PainChance "PainkillerIce", 0
	+NOTDMATCH
	+NOICEDEATH
	Tag "Spectre"
	ConversationID 67
	+BOSSDEATH
	DropItem ""
	Args 1
	States
	{
	See:
		TNT1 A 0 A_JumpIfInventory("DukeBossSee",1,"See2")
		TNT1 A 0 A_JumpIfInventory("CalebBossSee",1,"See2")
		TNT1 A 0 A_JumpIfInventory("EleenaBossSee",1,"See2")
		TNT1 A 0 A_JumpIfInventory("LoWangBossSee",1,"See2")
		TNT1 A 0 A_JumpIfInventory("LeonardBossSee",1,"See2")
		TNT1 A 0 A_JumpIfInventory("HLBossSee",1,"See2")
		TNT1 A 0 A_GiveInventory("DukeBossSee",1)
		TNT1 A 0 A_GiveInventory("CalebBossSee",1)
		TNT1 A 0 A_GiveInventory("EleenaBossSee",1)
		TNT1 A 0 A_GiveInventory("LoWangBossSee",1)
		TNT1 A 0 A_GiveInventory("LeonardBossSee",1)
		TNT1 A 0 A_GiveInventory("HLBossSee",1)
		TNT1 A 0 A_GiveToTarget("DukeBossSightTaunt")
		TNT1 A 0 A_GiveToTarget("CalebBossSightTaunt")
		TNT1 A 0 A_GiveToTarget("EleenaBossSightTaunt")
		TNT1 A 0 A_GiveToTarget("LoWangBossSightTaunt")
		TNT1 A 0 A_GiveToTarget("LeonardBossSightTaunt")
		TNT1 A 0 A_GiveToTarget("HLBossSightMusicQueue")

	See2:
		ALN1 AB 4 bright A_Chase
		ALN1 C 4 bright A_SentinelBob
		ALN1 DEF 4 bright A_FastChase
		ALN1 G 4 bright A_SentinelBob
		ALN1 HIJ 4 bright A_Chase
		ALN1 K 4 bright A_SentinelBob
		Loop

	Death:
		TNT1 A 0 ACS_NamedExecuteAlways("Samsara_KillCount", 0, 0)
		AL1P A 6 bright A_SpectreChunkSmall
		AL1P B 6 bright A_Scream
		AL1P C 6 bright A_SpectreChunkSmall
		AL1P DE 6 bright
		AL1P F 6 bright A_SpectreChunkSmall
		AL1P G 6 bright
		AL1P H 6 bright A_SpectreChunkSmall
		AL1P IJK 6 bright
		AL1P LM 5 bright
		AL1P N 5 bright A_SpectreChunkLarge
		AL1P OPQ 5 bright
		AL1P R 5 bright ACS_NamedExecuteAlways("SamsaraSpectres", 0, args[0])
		Stop

	Missile:
		ALN1 J 3 bright A_FaceTarget
		ALN1 I 3 bright A_SpotLightning
		ALN1 H 3 bright
		Goto See2

	Pain:
		TNT1 A 0 A_SpectreChunkSmall
		ALN1 J 1 A_Pain
		ALN1 J 1 A_Explode(64,64)
		TNT1 A 0 A_Jump(128,"Vanish")
		Goto See+6

	Vanish:
		TNT1 AAAAA 0 A_SpectreChunkSmall
		TNT1 A 0 A_UnsetShootable
		ALN1 J 1 A_SetTranslucent(0.5,1)
		ALN1 J 1 A_SetTranslucent(0.4,1)
		ALN1 J 1 A_SetTranslucent(0.3,1)
		ALN1 J 1 A_SetTranslucent(0.2,1)
		ALN1 J 1 A_SetTranslucent(0.1,1)
		TNT1 A 1

	Wandering:
		TNT1 AAAAAAAAAAAAAAAAAAAA 1 A_Wander
		TNT1 A 1 A_Pain
		TNT1 A 0 A_Jump(64,"SpawnFake")
		TNT1 A 0 A_Jump(128,"Wandering")
		ALN1 J 1 A_Explode(64,64)
		ALN1 J 1 A_SetTranslucent(0.1,1)
		ALN1 J 1 A_SetTranslucent(0.2,1)
		ALN1 J 1 A_SetTranslucent(0.3,1)
		ALN1 J 1 A_SetTranslucent(0.4,1)
		ALN1 J 1 A_SetTranslucent(0.5,1)
		ALN1 J 1 A_SetTranslucent(0.666,1)
		ALN1 A 1 A_SetShootable
		Goto See2

	SpawnFake:
		TNT1 A 1 A_SpawnItemEx("AlienSpectreFake",0,0,0,0,0,0,0,SXF_SETMASTER|SXF_NOCHECKPOSITION)
		Goto Wandering
	}
}

