//////////D64Guy
actor FriendlyDoomImp64 : DoomImp2
{
    Health 100
    Scale 0.70
    Height 64
    BloodType "Doom64Blood"
    SeeSound "friendlyimp64/sight"
    PainSound "friendlyimp64/pain"
    DeathSound "friendlyimp64/death"
    ActiveSound "friendlyimp64/active"
    MeleeSound "friendlyimp64/melee"
    MeleeDamage 3
    MissileType FriendlyDoomImp64Ball
    HitObituary "%o was somehow killed by a friendly Imp."
    Obituary "%o was somehow killed by a friendly Imp."
    Monster
    Species "Player"
    -SOLID
    +FRIENDLY    
    States
    {
      Spawn:
        64TR AB 12 A_Look
        loop
      See:
        64TR AABBCCDD 4 A_Chase
        Loop
      Melee:
        TNT1 A 0 A_ChangeFlag("NOPAIN",0)
        64TR EF 9 A_FaceTarget
        64TR G 8 A_MeleeAttack
        Goto See
      Missile:
        TNT1 A 0 A_ChangeFlag("NOPAIN",0)
        64TR IJ 9 A_FaceTarget
        64TR K 8 A_MissileAttack
        Goto See
      Pain:
        64TR H 2
        64TR H 2 A_Pain
        Goto See        
      Death:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
        64TR L 10
        64TR M 10 A_Scream
        64TR N 8
        64TR O 8 A_NoBlocking
        64TR P -1
        Stop
      Death.DukeExplosive:
      Death.DukeSpectral:
        TNT1 A 0 A_JumpIfHealthLower(-3,"DukeGibStart")
        goto Death.DukeDamage
      DukeGibStart:
        64TR H 1
        TNT1 A 0 A_UnsetSolid
        TNT1 A 0 A_SpawnDebris("DukeGibs1")
        TNT1 A 0 A_SpawnDebris("DukeGibs2")
        TNT1 A 0 A_SpawnDebris("DukeGibs3")
        TNT1 A 0 A_SpawnDebris("DukeGibs4")
        TNT1 A 0 A_SpawnDebris("DukeGibs5")
        TNT1 A 0 A_SpawnDebris("DukeGibs6")
        TNT1 A 0 A_PlaySound("duke/enemygib")
        TNT1 A 0 A_Jump(96,"XDeath")
        TNT1 A 0 A_GiveToTarget("DukeGibTaunt",1)
      XDeath:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
        64TR Q 6 A_PlaySound("friendlyimp64/gibbed", CHAN_VOICE)
        64TR R 6
        64TR S 6
        64TR T 6 A_NoBlocking
        64TR UVW 6
        64TR X -1
        Stop
      Death.DukeDamage:
      Death.DukePistol:
        TNT1 A 0 A_Jump(218,"Death")
        TNT1 A 0 A_GiveToTarget("DukeKillTaunt",1)
        goto Death
      Death.MightyBoot:
        TNT1 A 0 A_PlaySoundEx("duke/kickhit","body",0,0)
        TNT1 A 0 A_Jump(64,"Death")
        TNT1 A 0 A_GiveToTarget("DukeBootTaunt",1)
        goto Death
      Pain.MightyBoot:
        TNT1 A 0 A_ChangeFlag("NOPAIN",1)
        TNT1 A 0 A_PlaySoundEx("duke/kickhit","body",0,0)
        64TR H 2 A_FaceTarget
        TNT1 A 0 A_Recoil(14)
        64TR H 16 A_Pain
        Goto See
      Pain.MarathonStun:
        TNT1 A 0 A_Stop
        TNT1 A 0 A_GiveInventory("NothingSpeed")
        TNT1 A 0 A_PlaySoundEx("marathon/tech50/stun","body",0,0)
        64TR H 5 A_FaceTarget
        TNT1 A 0 A_Gravity
        64TR H 30 A_Pain
        64TR H 70
        Goto See
      Death.ZorchPistol:
      Death.Zorch:
      Death.ZorchKamikaze:
      Death.PhasingZorch:
      Death.ZorchSpectral:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
        64TR H 0 A_Stop
        TNT1 A 0 A_ChangeFlag("DONTGIB",1)
        TNT1 A 0 A_PlaySoundEx("chex/zorch","soundslot5",0,0)
        64TR H 3 A_SpawnItem("ZorchEffectSmall")
        TNT1 A 0 A_GiveInventory("EnemyZorched",1)
        64TR H 3 A_SetTranslucent(0.9,0)
        64TR H 0 A_Scream
        64TR H 3 A_SetTranslucent(0.8,0)
        64TR H 0 A_NoBlocking
        64TR H 3 A_SetTranslucent(0.7,0)
        64TR H 3 A_SetTranslucent(0.6,0)
        64TR H 3 A_SetTranslucent(0.5,0)
        64TR H 3 A_SetTranslucent(0.4,0)
        64TR H 3 A_SetTranslucent(0.3,0)
        64TR H 3 A_SetTranslucent(0.2,0)
        64TR H 3 A_SetTranslucent(0.1,0)
        TNT1 A 1 A_SetTranslucent(1,0)
        TNT1 A -1
        stop
      Death.DukeIce:
        TNT1 A 0 A_PlaySound("duke/enemyfreeze")
        TNT1 A 0 A_GiveInventory("FreezethrowerWeakness")
        TNT1 A 0 A_Jump(218,"Ice")
        TNT1 A 0 A_GiveToTarget("DukeKillTaunt",1)
      Ice:
      Death.HexenIce:
      Death.Ice:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
        TNT1 A 0 A_GiveInventory("EnemyZorched",1)
        TNT1 A 0 A_NoBlocking
        //TNT1 A 0 A_SpawnItem("ImpFrozen")
        64TR H 0 A_GenericFreezeDeath
        64TR H 70 A_UnsetSolid
        64TR H 0 A_IceGuyDie
        64TR H 1 A_IceGuyDie
        TNT1 A -1
        stop
      Crush:
        64GB A 0 A_PlaySound("friendlyimp64/crush")
        64GB A -1
        Stop
    }
}

