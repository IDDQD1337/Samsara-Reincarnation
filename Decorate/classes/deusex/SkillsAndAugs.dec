Actor DeusEx_SpyDrone
{
	Height 12
	Radius 8
	Species "Player"
	Health 100
    Tag "Spy Drone"
	+NOGRAVITY
	+SOLID
	+THRUSPECIES
	+SHOOTABLE
	+NOBLOCKMONST
	+NODROPOFF
	+CANPASS
	States
	{
		Spawn:
			DX00 A 1 A_Playsound("DeusEx/SpyDrone/Move",CHAN_5,1.0,true)
			Loop
		Death:
			TNT1 A 0 A_StopSound(CHAN_5)
			TNT1 A 0 A_GiveInventory("DeusEx_Augmentation_SpyDroneDetonate",1,AAPTR_MASTER)
			TNT1 A 0 A_JumpIfInventory("DeusEx_Augmentation_SpyDrone",4,"Explode4",AAPTR_MASTER)
			TNT1 A 0 A_JumpIfInventory("DeusEx_Augmentation_SpyDrone",3,"Explode3",AAPTR_MASTER)
			TNT1 A 0 A_JumpIfInventory("DeusEx_Augmentation_SpyDrone",2,"Explode2",AAPTR_MASTER)
		Explode1:
			TNT1 A 1 A_CustomMissile("DeusEx_DroneExplosion1",0)
			Stop
		Explode2:
			TNT1 A 1 A_CustomMissile("DeusEx_DroneExplosion2",0)
			Stop
		Explode3:
			TNT1 A 1 A_CustomMissile("DeusEx_DroneExplosion3",0)
			Stop
		Explode4:
			TNT1 A 1 A_CustomMissile("DeusEx_DroneExplosion4",0)
			Stop
		StopIt:
			DX00 A 0 A_GiveInventory("DeusEx_Augmentation_SpyDroneDetonate",1,AAPTR_MASTER)
			DX00 A 0 A_StopSound(CHAN_5)
			DX00 A 0 A_Gravity
			DX00 A 1 A_CheckFloor("StopFloor")
			wait
		StopFloor:
			DX00 A 1 A_Playsound("DeusEx/SpyDrone/Death",CHAN_5,1.0,true)
			DX00 AAAAAAAAAAAAAAAAAA 3 A_SpawnItemEx("DeusEx_SmokeTrail",0,0,0,frandom(-0.25,0.25),frandom(-0.25,0.25),1.0)
			DX00 A 2 A_Fadeout(0.025)
			wait
	}
}

