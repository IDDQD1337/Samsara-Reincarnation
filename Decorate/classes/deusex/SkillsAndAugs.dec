Actor DeusEx_Augmentation_ObjectGrabberDamageNearbyActors
{
	Projectile
	Damage (0)
	Speed 0
	ReactionTime 1
	+THRUACTORS
	States
	{
		Spawn:
			TNT1 A 1 A_CountDown
			Loop
		Death:
			TNT1 A 1 A_Explode(mass,score,0,0,score)
			Stop
	}
}

