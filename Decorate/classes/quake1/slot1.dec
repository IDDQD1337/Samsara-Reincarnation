actor MjolnirCheckPuff: BulletPuff
{
    VSpeed 0
    +PUFFONACTORS
    +PUFFGETSOWNER
    +BLOODLESSIMPACT
    +NOTIMEFREEZE
    States
    {
      Spawn:
      Crash:
      XDeath:
        TNT1 A 1
        TNT1 A 35 A_GiveToTarget("MjolnirCrash")
        stop
    }
}

