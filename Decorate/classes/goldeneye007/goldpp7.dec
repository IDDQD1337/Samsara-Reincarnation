}

Actor Goldfinger_GoldMagnumPuff : Goldeneye_BulletPuff
{
	Damage (1000)
    States
    {
      Spawn:
        TNT1 A 0
        TNT1 A 0 A_SetUserVar("user_damage",1000)
        Goto Super::Spawn
      XDeath:
        TNT1 A 0 A_SetUserVar("user_damage",1000)
        Goto Super::XDeath
    }
}
