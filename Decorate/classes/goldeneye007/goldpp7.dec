actor SamsaraGoldfingerGoldMagnumSingleAttackHandler : Trigger
{
    States
    {
      // Ammo checking/taking stuff
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"PickupReloadable")
      PickupNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("Goldeneye_GoldenGunRounds",1,1)
        stop
        TNT1 A 0 A_TakeInventory("Goldeneye_GoldenGunRounds",1)
        goto PickupAttack

      PickupReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("Goldfinger_GoldMagnumMagazine",1,1)
        stop
        TNT1 A 0 A_TakeInventory("Goldfinger_GoldMagnumMagazine",1)
        goto PickupAttack

      // Attack stuff
      PickupAttack:
		TNT1 A 0 ACS_NamedExecuteWithResult("BondAltFireActivator")
		TNT1 A 0 A_GiveInventory("BondAccuracyX",random(0,500))
		TNT1 A 0 A_GiveInventory("BondAccuracyY",random(0,500))
        TNT1 A 0 A_PlaySound("Goldfinger/MagnumShot",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackCoop")
      PickupAttackDM:
		TNT1 A 0 A_RailAttack(0,6,false,"None","None",RGF_SILENT|RGF_EXPLICITANGLE|32,0,"Goldfinger_GoldMagnumPuff",((CallACS("BondAccuracy")-250)/100.0),((CallACS("BondAccuracy",1)-250)/100.0),4096)
		TNT1 A 0 A_FireCustomMissile("Goldeneye_CougarTracer",((CallACS("BondAccuracy")-250)/100.0),false,6,0,0,-((CallACS("BondAccuracy",1)-250)/100.0))
		TNT1 A 0 A_TakeInventory("BondAccuracyX")
		TNT1 A 0 A_TakeInventory("BondAccuracyY")
        stop

      PickupAttackCoop:
		TNT1 A 0 A_RailAttack(0,6,false,"None","None",RGF_SILENT|RGF_EXPLICITANGLE|32,0,"Goldfinger_GoldMagnumPuff",((CallACS("BondAccuracy")-250)/100.0),((CallACS("BondAccuracy",1)-250)/100.0),4096)
		TNT1 A 0 A_FireCustomMissile("Goldeneye_CougarTracerCoop",((CallACS("BondAccuracy")-250)/100.0),false,6,0,0,-((CallACS("BondAccuracy",1)-250)/100.0))
		TNT1 A 0 A_TakeInventory("BondAccuracyX")
		TNT1 A 0 A_TakeInventory("BondAccuracyY")
        stop
    }
}

