actor Goldfinger_GoldMagnumMagazine : Ammo
{
    Inventory.MaxAmount 6
    Tag "Golden Magnum Cylinder (Right)"
    +IGNORESKILL
}

actor Goldfinger_GoldMagnumDualMagazine : Ammo
{
    Inventory.MaxAmount 6
