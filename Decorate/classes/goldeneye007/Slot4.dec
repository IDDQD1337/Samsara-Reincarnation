        GE02 B 0 A_JumpIfInventory("BondGunAngle",0,"FireRight5")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",9,"FireRight4")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",8,"FireRight3")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",7,"FireRight2")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",6,"FireRight1")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",5,"FireCenter")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",4,"FireLeft1")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",3,"FireLeft2")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",2,"FireLeft3")
        GE02 B 0 A_JumpIfInventory("BondGunAngle",1,"FireLeft4")
        goto FireCenter

      FireRight5:
        GE07 B 0 A_GiveInventory("BondGunAngleFiringSingle",10)
        GE07 B 1 Bright A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE07 C 0 A_JumpIfInventory("SamsaraReloadMode",2,"FireRight5Reloadable")
      FireRight5NonReloadable:
        GE07 C 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"FireRight5Attack")
        GE07 C 0 A_JumpIfInventory("Clip",1,"FireRight5Attack")
