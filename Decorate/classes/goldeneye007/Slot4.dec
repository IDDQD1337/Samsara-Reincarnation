actor Goldeneye_AR33Tracer : Goldeneye_BulletTracerThruGhost
{
    Damage (14)
    States
    {
      Spawn:
        GE00 A 0 Bright
        GE00 A -1 Bright A_ChangeVelocity(velx*2,vely*2,velz*2,CVF_REPLACE)
        stop

      XDeath:
        TNT1 A 0 A_SetUserVar("user_damage",14)
        goto Super::XDeath
    }
}

