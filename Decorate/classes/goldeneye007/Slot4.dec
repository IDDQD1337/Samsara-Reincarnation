        goto FireRight5Continue1

      FireRight5Attack:
        GE07 C 0 A_SpawnItemEx("Goldeneye_Cartridge_Rifle",8,-12,36,1,0,0,-100,SXF_NOCHECKPOSITION|SXF_MULTIPLYSPEED|SXF_TRANSFERPITCH)
      FireRight5Continue1:
        GE07 C 0 A_GiveInventory("SamsaraGoldenEyeAR33SingleAttackHandler")
        GE07 C 1 Bright A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE07 D 0 A_Jump(128,2)
        GE07 D 1 Bright A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        GE07 E 0 A_TakeInventory("BondFiringToken")
      FireRight5BurstCheck:
        GE07 E 0 A_JumpIfInventory("BondZoomToken",1,"FireRight5ReFireCheck")
        GE07 E 0 A_JumpIfInventory("SamsaraGoldenEyeAR33UnzoomedSingleAttackAmount",0,"FireRight5ReFireCheck")
        GE07 E 0 A_JumpIfInventory("SamsaraReloadMode",2,"FireRight5BurstCheckReloadable")
      FireRight5BurstCheckNonReloadable:
