        GE07 E 0 A_JumpIfInventory("Goldeneye_AR33Magazine",1,"FireRight5ReFire")
        goto FireRight5Continue2

      FireRight5ReFire:
        GE07 E 0 A_ReFire(1)
        goto FireRight5Continue2
        GE07 E 0 A_GiveInventory("BondFireDelay")
        GE07 E 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto Fire

      FireRight5Continue2:
        GE07 EFGHIJK 1 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
        goto FireSetDualOrder

      FireRight4:
        GE08 B 0 A_GiveInventory("BondGunAngleFiringSingle",9)
