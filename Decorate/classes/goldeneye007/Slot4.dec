        GE02 B 0 A_JumpIfInventory("BondZoomToken",1,"FireAltZoomCheck")
        GE02 B 0 A_JumpIfInventory("BondAltActivationToken",1,"FireAltZoomCheck")
        GE02 B 0 A_TakeInventory("BondAltToggle")
        GE02 B 0 A_GiveInventory("BondAltActivationToken")
        GE02 B 0 ACS_NamedExecuteAlways("BondAltFire")
