// Tokens

actor Goldeneye_AR33PickedUpX : MapCoordinate {}
actor Goldeneye_AR33PickedUpY : MapCoordinate {}
actor Goldeneye_AR33PickedUpZ : MapCoordinate {}
actor SamsaraGoldenEyeAR33UnzoomedSingleAttackAmount : Counter { Inventory.MaxAmount 3 }
actor SamsaraGoldenEyeAR33UnzoomedDualAttackAmount : Counter { Inventory.MaxAmount 3 }

// Weapon

actor Goldeneye_AR33 : SamsaraGoldenEyeWeapon
{
    Inventory.PickupMessage "Picked up an AR33 assault rifle."
    Weapon.SlotNumber 4
    Weapon.SelectionOrder 13
    Weapon.AmmoType1 "Goldeneye_AR33Magazine"
    Weapon.AmmoUse1 1
    Weapon.AmmoType2 "Clip"
    Weapon.AmmoGive2 30
    Obituary "%o was drilled by %k's AR33."
    Tag "US AR33 Assault Rifle"
    States
    {
      Spawn:
        PIST A -1
        stop

      // Single
      Ready:
        TNT1 A 0 A_JumpIfInventory("BondWatchQueue",1,"WatchRaise")
        TNT1 A 0 A_JumpIfInventory("BondWatchToken",1,"WatchReady")
        TNT1 A 0 A_JumpIfInventory("BondAltToggle",2,"ZoomWeapon")
        TNT1 A 0 A_JumpIfInventory("BondAltToggle",1,"DualWield")
        TNT1 A 0 A_JumpIfInventory("BondZoomToken",1,"DecideZoomWeapon")
      ReadyStart:
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",0,"ReadyRight5")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",9,"ReadyRight4")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",8,"ReadyRight3")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",7,"ReadyRight2")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",6,"ReadyRight1")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",5,"ReadyCenter")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",4,"ReadyLeft1")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",3,"ReadyLeft2")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",2,"ReadyLeft3")
        TNT1 A 0 A_JumpIfInventory("BondGunAngle",1,"ReadyLeft4")
        goto ReadyCenter

      ReadyRight5:
        GE01 E 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyRight5Reloadable")
      ReadyRight5NonReloadable:
        GE01 E 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyRight5Reloadable:
        GE01 E 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyRight4:
        GE01 D 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyRight4Reloadable")
      ReadyRight4NonReloadable:
        GE01 D 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyRight4Reloadable:
        GE01 D 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyRight3:
        GE01 C 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyRight3Reloadable")
      ReadyRight3NonReloadable:
        GE01 C 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyRight3Reloadable:
        GE01 C 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyRight2:
        GE01 B 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyRight2Reloadable")
      ReadyRight2NonReloadable:
        GE01 B 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyRight2Reloadable:
        GE01 B 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyRight1:
        GE01 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyRight1Reloadable")
      ReadyRight1NonReloadable:
        GE01 A 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyRight1Reloadable:
        GE01 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyCenter:
        GE00 A 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyCenterReloadable")
      ReadyCenterNonReloadable:
        GE00 A 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyCenterReloadable:
        GE00 A 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyLeft1:
        GE00 B 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyLeft1Reloadable")
      ReadyLeft1NonReloadable:
        GE00 B 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyLeft1Reloadable:
        GE00 B 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyLeft2:
        GE00 C 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyLeft2Reloadable")
      ReadyLeft2NonReloadable:
        GE00 C 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyLeft2Reloadable:
        GE00 C 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyLeft3:
        GE00 D 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyLeft3Reloadable")
      ReadyLeft3NonReloadable:
        GE00 D 1 A_WeaponReady
        goto ReadyAmmoCheck

      ReadyLeft3Reloadable:
        GE00 D 1 A_WeaponReady(WRF_ALLOWRELOAD)
        goto ReadyAmmoCheck

      ReadyLeft4:
        GE00 E 0 A_JumpIfInventory("SamsaraReloadMode",2,"ReadyLeft4Reloadable")
