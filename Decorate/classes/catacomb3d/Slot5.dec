}

Actor Catacomb_NukeProjectile : Catacomb_MagicMissileProjectile
{
    Speed 15
    Damage (0)
    Radius 10
    Height 10
    Scale 1.50
    DeathSound "Catacomb/BigExplosion"
    +FORCERADIUSDMG
    DamageType "FireExplosive"
    States
    {
        Spawn:
            CTA1 AB 2 Bright
            Loop
