actor Catacomb_XTerminatorsQuickSpell : Catacomb3D_QuickSpell
{
    Inventory.PickupMessage "XTerminators"
    Inventory.PickupSound "Catacomb/ItemGet2"
    Weapon.UpSound "Catacomb/ItemGet2"
    Weapon.SlotNumber 2
    Weapon.SelectionOrder 2900
    Weapon.AmmoType "Catacomb_XTerminators"
    Weapon.AmmoGive 4
    Weapon.AmmoUse 1
    Tag "XTerminators"
    States
    {
      Spawn:
        CT01 ABC 8 Bright
        loop

      Ready:
        goto ReadyRaise

      Fire:
        TNT1 A 0 A_JumpIfInventory("Catacomb_WeaponMode",1,"AltFireRelease")
        goto FireMagicMissile

      AltFire:
        goto AltFireCharge

      AltFireRelease:
        TNT1 A 0 A_AlertMonsters
        TNT1 A 0 A_JumpIfInventory("PowerCatacomb_BounceShot",1,"AltFireReleaseBounce")
        TNT1 A 0 A_JumpIfInventory("PowerCatacomb_RapidFire",1,"AltFireReleaseFast")
        TNT1 A 0 A_JumpIfInventory("PowerCatacomb_SpreadShot",1,"AltFireReleaseSpread")
        TNT1 A 0 A_JumpIfInventory("PowerCatacomb_FireRing",1,"AltFireReleaseFireball")
      AltFireReleaseNormal:
        CT00 B 10 Bright A_GiveInventory("SamsaraCatacombXTerminatorsAttackHandler")
        CT00 A 10
        goto AltFireFinish

      AltFireReleaseFireball:
        CT00 D 10 Bright A_GiveInventory("SamsaraCatacombXTerminatorsFireballAttackHandler")
        CT00 C 10
        goto AltFireFinish

      AltFireReleaseSpread:
        CT00 F 3 Bright A_GiveInventory("SamsaraCatacombXTerminatorsSpreadAttackHandler1")
        CT00 F 3 Bright A_GiveInventory("SamsaraCatacombXTerminatorsSpreadAttackHandler2")
        CT00 F 4 Bright A_GiveInventory("SamsaraCatacombXTerminatorsSpreadAttackHandler3")
        CT00 E 10
        goto AltFireFinish

      AltFireReleaseFast:
        CT00 H 10 Bright A_GiveInventory("SamsaraCatacombXTerminatorsFastAttackHandler")
        CT00 G 10
        goto AltFireFinish

      AltFireReleaseBounce:
        CT00 J 10 Bright A_GiveInventory("SamsaraCatacombXTerminatorsBounceAttackHandler")
        CT00 I 10
        goto AltFireFinish

      AltFireFinish:
        TNT1 A 0 A_CheckReload
        goto ReadyActual
    }
}

