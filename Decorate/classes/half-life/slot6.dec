      ReloadStartShephard:
        OP10 A 1 A_WeaponReady(WRF_NOFIRE)
        OP10 B 0 A_PlaySound("HLSaw/Reload")
        OP10 BDE 1 A_WeaponReady(WRF_NOFIRE)
        OP10 F 0
        OP10 GHJKLMNPQSTVWXYZ 1 A_WeaponReady(WRF_NOFIRE)
