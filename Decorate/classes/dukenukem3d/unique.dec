	+ISMONSTER
	-COUNTKILL
	-SHOOTABLE
	States
	{
		Spawn:
		Death:
			TNT1 A 0
			TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraDecorate",36,1) == 0, 2)
			TNT1 A 0 A_SetDamageType("ExpanderDMG")
			TNT1 A 1 A_Explode(CallACS("Samsara_Expander",4), CallACS("Samsara_Expander",4)*2, XF_NOTMISSILE, 1, 0)
			stop
	}
}
