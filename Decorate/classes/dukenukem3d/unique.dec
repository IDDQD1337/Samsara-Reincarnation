// Shrink Projectile.

ACTOR ShrinkMissile
{
	Scale 0.5
	Radius 8
	DamageType "ShrinkerDMG"
	Height 8
	+FORCEPAIN
	+HITTRACER
	+BLOODLESSIMPACT
	+DONTSPLASH
	-BLOODSPLATTER
	Speed 40
	Damage 0
	Projectile
	//Obituary "$OB_MPROCKET" // "%o rode %k's rocket."
	States
	{
		Spawn:
			DKSR JKLM 1 Bright
			Loop
		Crash:
		Death:
			DKSR NOPQ 4 Bright
			Stop
		XDeath:
			TNT1 A 0 A_Explode(1,68,0)
			Goto Death
	}
}

Actor ShrinkMissileCoop : ShrinkMissile { +THRUSPECIES +MTHRUSPECIES Species "Player" +DONTHARMSPECIES }

Actor ShrinkMissile64 : ShrinkMissile
{
	States
	{
		Spawn:
			D6XP EFGH 1 Bright
			Loop
		Crash:
		Death:
			TNT1 A 0 A_PlaySound("duke/thud")
			TNT1 A 0 A_SpawnItemEx("ShrinkMissile64FireBall",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_CLIENTSIDE)
			D6XP IJKL 4 Bright
			Stop
		XDeath:
			TNT1 A 0 A_Explode(1,68,0)
			Goto Death
	}
}

