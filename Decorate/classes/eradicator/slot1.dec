		ER75 U 2 A_SeekerMissile(90,90,SMF_LOOK)
		ER75 V 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,"SpawnRemote")
		ER75 V 2 A_SeekerMissile(90,90,SMF_LOOK)
		ER75 W 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,"SpawnRemote")
		ER75 W 2 A_SeekerMissile(90,90,SMF_LOOK)
		ER75 X 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,"SpawnRemote")
		ER75 X 2 A_SeekerMissile(90,90,SMF_LOOK)
        Loop
	  SpawnRemote:
		ER75 T 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,1)
		Goto SpawnLoop
        ER75 T 2
		ER75 U 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,1)
		Goto SpawnLoop
        ER75 U 2
		ER75 V 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,1)
		Goto SpawnLoop
        ER75 V 2
		ER75 W 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,1)
		Goto SpawnLoop
        ER75 W 2
		ER75 X 0 A_JumpIfInTargetInventory("EradicatorPipAllowControl",1,1)
		Goto SpawnLoop
        ER75 X 2
        Loop

      Death:
		TNT1 A 0 A_Stop
        TNT1 A 0 A_PlaySound("silence",CHAN_VOICE)
        TNT1 A 0 A_StopSound(CHAN_VOICE)
        TNT1 A 0 A_PlaySound("Eleena/SpiderDie",CHAN_VOICE)
        TNT1 A 0 A_SpawnDebris("EradFriendlyRemoteCarGib")
        TNT1 A 0 A_SpawnDebris("EradFriendlyRemoteCarGib")
        TNT1 A 0 A_SpawnDebris("EradFriendlyRemoteCarGib")
        TNT1 A 0 A_SpawnDebris("EradFriendlyRemoteCarGib")
        GLBB A 2 Bright Light("ERADEXPLOSION_1__") A_Explode(175,55,XF_HURTSOURCE,true)
        GLBB B 2 Bright Light("ERADEXPLOSION_2__")
        GLBB C 2 Bright Light("ERADEXPLOSION_3__")
        GLBB D 2 Bright Light("ERADEXPLOSION_4__") A_FadeOut
        GLBB EF 2 Bright Light("ERADEXPLOSION_5__") A_FadeOut
		TNT1 A 1 ACS_NamedExecuteWithResult("EradicatorPipObjectClear")
        stop
    }
}

