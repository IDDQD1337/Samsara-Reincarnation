            UN03 G 0 A_JumpIfInventory("Cell",1,1)
            goto WindDown
            UN03 G 0 A_JumpIfInventory("SynthFireRight",1,1)
            goto WindDown
			UN03 G 0 A_AlertMonsters
			UN03 G 0 A_SpawnItemEx("UnrealBrassShell",-8,-12,22,3,0,0,90,SXF_NOCHECKPOSITION|SXF_MULTIPLYSPEED|SXF_TRANSFERPITCH)
			UN03 G 0 A_GiveInventory("SamsaraUnrealMinigunSecondaryAttackHandler")
			UN03 GH 2 Bright A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
