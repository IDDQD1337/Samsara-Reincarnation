        TNT1 A 0 A_GiveInventory("Shell",15)
        TNT1 A 0 A_GiveInventory("RocketAmmo",15)
        TNT1 A 0 A_GiveInventory("Cell",15)
		TNT1 A 0 A_GiveInventory("StrifePoisonAmmo", 8)
		TNT1 A 0 A_GiveInventory("WhitePhosGrenade", 4)
		Stop
		
	  PickupRott:
		TNT1 A 0 A_GiveInventory("FBMissiles", 4)		
	    TNT1 A 0 A_GiveInventory("SMissiles", 10)	
		TNT1 A 0 A_GiveInventory("HSMissiles", 10)	
		TNT1 A 0 A_GiveInventory("RottMissiles", 15)
	    TNT1 A 0 A_JumpIfInventory("DMissiles", 0, "Check1")
		TNT1 A 0 A_JumpIfInventory("FWMissiles", 0, "Check2")
      RottSafe:
		TNT1 A 0 A_GiveInventory("DMissiles", 20)
		TNT1 A 0 A_GiveInventory("FWMissiles", 5)
	    Stop
	  Check1:
		TNT1 A 0 A_JumpIfInventory("FWMissiles", 0, "PickupFail")
		Goto RottSafe
	  Check2:
		TNT1 A 0 A_JumpIfInventory("DMissiles", 0, "PickupFail")
	    Goto RottSafe

	  PickupBlood:
        TNT1 A 0 A_JumpIfInventory("Clip", 0, 2)
		TNT1 A 0 A_GiveInventory("Clip", 15)
		TNT1 A 0 A_JumpIfInventory("Shell", 0, 2)
		TNT1 A 0 A_GiveInventory("Shell", 15)
		TNT1 A 0 A_JumpIfInventory("Cell", 0, 2)
		TNT1 A 0 A_GiveInventory("Cell", 15)	
		TNT1 A 0 A_JumpIfInventory("RocketAmmo", 0, 2)
		TNT1 A 0 A_GiveInventory("RocketAmmo", 15)	
		TNT1 A 0 A_JumpIfInventory("SprayCanAmmo", 0, "PickupFail")
        TNT1 A 0 A_GiveInventory("SprayCanAmmo", 72)
        stop
		
      PickupMarathon:
        TNT1 A 0 A_GiveInventory("Clip",15)
        TNT1 A 0 A_GiveInventory("Shell",15)
        TNT1 A 0 A_GiveInventory("RocketAmmo",15)
        TNT1 A 0 A_GiveInventory("Cell",15)
        TNT1 A 0 A_SetBlend("Green",0.4,4)
        stop

      PickupWolfen:
        TNT1 A 0 A_GiveInventory("Clip",30)
        TNT1 A 0 A_GiveInventory("RocketAmmo",15)
        TNT1 A 0 A_GiveInventory("Cell",15)
        stop

      PickupHexen:
        TNT1 A 0 A_GiveInventory("Clip",30)
        TNT1 A 0 A_GiveInventory("Cell",30)
        stop

      PickupQuake:
