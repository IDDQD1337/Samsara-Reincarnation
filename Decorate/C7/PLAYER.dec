			"####" "#" 0 A_JumpIfInventory("C7MineDMToken",0,"TooManyMines")
			"####" "#" 0 A_JumpIfNoAmmo("Ready")
			"####" "#" 0 A_GiveInventory("C7MineDMToken",1)
			"####" "#" 5 ACS_NamedExecuteWithResult("C7MinePlacer",1)
			"####" "#" 5 A_TakeInventory("C7ProxyMineAmmo", 1)
			"####" "#" 5 A_Print("Mine placed")
			"####" "#" -1 A_Jump(256,"Ready")
			Wait
		TooManyMines:
			"####" "#" 5 A_Print("You may not have more than 20 mines active.")
			"####" "#" -1 A_Jump(256,"Ready")
			Wait
	}
}

