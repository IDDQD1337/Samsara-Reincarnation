			"####" "#" 5 ACS_NamedExecuteWithResult("C7MinePlacer") 
			"####" "#" 5 A_TakeInventory("C7ProxyMineAmmo", 1)
			"####" "#" 5 A_Print("Mine placed")
			"####" "#" -1 A_Jump(256,"Ready")
