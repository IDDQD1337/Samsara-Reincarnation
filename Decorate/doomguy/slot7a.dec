// Stronghold Devastator
actor SamsaraDoomguyStrDevastatorHeldCheck : Boolean {}

actor " Devastator " : Weapon
{
    //Weapon.SlotNumber 7
	Inventory.PickupMessage "You got the devastator! Hell yeah!"
    Weapon.AmmoType "Cell"
    Weapon.AmmoGive 0 //60
    Weapon.AmmoUse 60
    Tag "Devastator"
	Inventory.RestrictedTo "DoomguyPlayer"
    +BFG
    +EXPLOSIVE
    +NOAUTOFIRE
    +UNDROPPABLE
    States
    {
      Spawn:
        WDEV A -1
        stop

      Ready:
        DEVG A 1 A_WeaponReady
        loop

      Deselect:
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPower",1,"DeselectPowered")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPowerPerma",1,"DeselectPowered")
      DeselectContinue:
        TNT1 A 0 A_Lower
        DEVG A 1 A_Lower
        goto Deselect

      DeselectPowered:
        TNT1 A 0 A_Lower
        goto DeselectContinue

      Select:
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPower",1,"SelectPowered")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPowerPerma",1,"SelectPowered")
      SelectContinue:
        TNT1 A 0 A_Raise
        DEVG A 1 A_Raise
        goto Select

      SelectPowered:
        TNT1 A 0 A_Raise
        goto SelectContinue

      // Unpowered
      Fire:
        TNT1 A 0 A_TakeInventory("SamsaraDoomguyStrDevastatorHeldCheck")
        TNT1 A 0 A_PlaySound("doomguy/stronghold/devchr",CHAN_WEAPON)
        DEVG BCDEF 10 Bright
      FireContinue:
        TNT1 A 0 A_WeaponReady(WRF_NOBOB|WRF_NOSWITCH|WRF_NOFIRE)
        TNT1 A 0 A_ReFire
        DEVG J 5 Bright A_GunFlash
        TNT1 A 0 A_PlaySound("doomguy/stronghold/devfir",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrDevastatorHeldCheck",1,"HoldContinue")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPower",1,"FireContinuePowered")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPowerPerma",1,"FireContinuePowered")
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorBall")
      FireFinish:
        DEVG K 10 Bright
        DEVG L 7 Bright
        goto Ready

      Hold:
        TNT1 A 0 A_GiveInventory("SamsaraDoomguyStrDevastatorHeldCheck")
        TNT1 A 0 A_PlaySound("doomguy/stronghold/devfch",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPower",1,"HoldPowered")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPowerPerma",1,"HoldPowered")
        DEVG GHIGHI 4 Bright A_FireBullets(4,4,4,4,"SamsaraDoomguyStrDevPuff",0,256)
        goto FireContinue

      HoldContinue:
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPower",1,"HoldContinuePowered")
        TNT1 A 0 A_JumpIfInventory("SamsaraDoomguyStrWeaponPowerPerma",1,"HoldContinuePowered")
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorBall1")
        goto FireFinish

      // Powered
      FireContinuePowered:
        TNT1 A 0 A_GiveInventory("SamsaraDoomguyStrDevastatorPoweredFireAttackHandler")
        goto FireFinish

      HoldPowered:
        DEVG GHIGHI 4 Bright A_FireBullets(4,4,4,4,"SamsaraDoomguyStrDevPuff",0,random(256,512))
        goto FireContinue

      HoldContinuePowered:
        TNT1 A 0 A_GiveInventory("SamsaraDoomguyStrDevastatorPoweredHoldAttackHandler")
        goto FireFinish

      Flash:
        TNT1 A 5 A_Light1
        TNT1 A 10 A_Light2
        TNT1 A 7 A_Light1
        goto LightDone
    }
}

actor SamsaraDoomguyStrDevastatorPoweredFireAttackHandler : Trigger
{
    States
    {
      Pickup:
      PickupFirst:
        TNT1 A 0 A_FireCustomMissile("SamsaraDoomguyStrDevastatorCannonProjectile",0,false)
      PickupSecondCheck:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupSecond")
        TNT1 A 0 A_JumpIfInventory("Cell",60,"PickupSecond")
