        DGUY G 1 A_PlaySoundEx("duke/kickhit","body",0,0)
        TNT1 A 0 A_Recoil(12)
        DGUY G 6 A_Pain
        Goto See
	  Pain.MarathonStun:
		TNT1 A 0 A_JumpIfInventory("Doom64Mode", 1, "Pain64.MarathonStun")
		TNT1 A 0 A_SetScale(1.0, 1.0)
