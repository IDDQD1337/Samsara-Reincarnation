actor Doom64BulletPuff : BulletPuff
{
    Scale 0.82
    VSpeed 0.7
    RenderStyle Normal
    states
    {
      Spawn:
        64PF A 4 bright
        64PF B 4 bright
        // Intentional fall-through
      Melee:
        64PF CDEF 4
        stop
    }
}

actor Doom64Bullet : DoomBullet
{
    Decal BulletChip
    States
    {
      Death:
        TNT1 A 1 A_SpawnItem("Doom64BulletPuff")
        stop
    }
}

