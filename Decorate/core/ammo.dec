/* AMMO NOTES

* IP Freely can not be balanced by regular ammo limits all other classes have, thus we introduce a new method.
* I guess that doesn't have any issues since ammos are all given through custominventories and you do pick em up all the time anyway.

- Clips give 1-5 drunk missile ammo.
- Shells give 5-15 bazooka ammo.
- Rockets give 1-5 firebomb ammo.
- Cells give 20-60 split missile ammo and 1-5 flamewall ammo*/

// Regular Ammo Pickups
actor Ammo1Small : CustomInventory replaces Clip// replaces Clip
{
	-INVENTORY.ALWAYSPICKUP
    -COUNTITEM
	+INVENTORY.QUIET
	//Inventory.PickupMessage "$SAMSARA_CLIP_1"
	Inventory.PickupSound "nosoundforyoubitch"
	States
	{
	Spawn:
		AMSM A 20
		AMSM A 20 BRIGHT
		Loop
	SpawnCancel:
		TNT1 A 1 A_FadeOut
		stop
	Pickup:
		TNT1 A 0 A_JumpIfInventory("QuakeClass", 0, "CheckQuake")
		TNT1 A 0 A_JumpIfInventory("RottClass", 0, "CheckRott")
		TNT1 A 0 A_JumpIfInventory("DisruptorClass", 0, "CheckDisruptor")
		TNT1 A 0 A_JumpIfInventory("RRClass", 0, "CheckRedneck")
		TNT1 A 0 A_JumpIfInventory("BittermanClass", 0, "CheckBitterman")
		TNT1 A 0 A_JumpIfInventory("CatacombClass", 0, "CheckCatacomb")
		TNT1 A 0 A_JumpIfInventory("PainkillerClass", 0, "CheckPainkiller")
		TNT1 A 0 A_JumpIfInventory("RTCWClass", 0, "CheckRTCW")
		TNT1 A 0 A_JumpIfInventory("Q3Class", 0, "CheckSarge")
		TNT1 A -1 A_Jump(256,"CheckNormal")
		wait
	CheckNormal:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, "PickupFail")
		goto PickupActual
	CheckQuake:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("LavaNails", 0, "PickupFail")
		Goto PickupActual
	CheckRott:
		TNT1 A 0 A_JumpIfInventory("HSMissiles", 0, "PickupFail")
		goto PickupActual
	CheckDisruptor:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("DisruptorHiFreq", 0, "PickupFail")
		Goto PickupActual
	CheckRedneck:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("MotoGunAmmo", 0, "PickupFail")
		Goto PickupActual
	CheckBitterman:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("Q2Flechettes", 0, "PickupFail")
		Goto PickupActual
	CheckCatacomb:
		TNT1 A 0 A_JumpIfInventory("Catacomb_Waves", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("Catacomb_Zappers", 0, "PickupFail")
		Goto PickupActual
	CheckPainkiller:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("Painkiller_FlamerAmmo", 0, "PickupFail")
		Goto PickupActual
	CheckRTCW:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("RTCW_AlliedAmmo1", 0, "PickupFail")
		Goto PickupActual
	CheckSarge:
		TNT1 A 0 A_JumpIfInventory("Clip", 0, 1)
		Goto PickupActual
		TNT1 A 0 A_JumpIfInventory("Q3ChaingunAmmo", 0, "PickupFail")
		Goto PickupActual
	PickupActual:
		TNT1 A 0 A_JumpIfInventory("MarathonClass", 0, "PickupMarathon")
		TNT1 A 0 A_JumpIfInventory("QuakeClass", 0, "PickupQuake")
		TNT1 A 0 A_JumpIfInventory("RottClass", 0, "PickupRott")
		TNT1 A 0 A_JumpIfInventory("BlakeClass", 0, "PickupBlake")
		TNT1 A 0 A_JumpIfInventory("C7Class", 0, "PickupBlake")
		TNT1 A 0 A_JumpIfInventory("DisruptorClass", 0, "PickupDisruptor")
		TNT1 A 0 A_JumpIfInventory("WitchavenClass", 0, "PickupBlake")
		TNT1 A 0 A_JumpIfInventory("RRClass", 0, "PickupRedneck")
		TNT1 A 0 A_JumpIfInventory("BittermanClass", 0, "PickupBitterman")
		TNT1 A 0 A_JumpIfInventory("CatacombClass", 0, "PickupCatacomb")
		TNT1 A 0 A_JumpIfInventory("PainkillerClass", 0, "PickupPainkiller")
		TNT1 A 0 A_JumpIfInventory("RTCWClass", 0, "PickupRTCW")
		TNT1 A 0 A_JumpIfInventory("Q3Class", 0, "PickupSarge")
		TNT1 A -1 A_Jump(256,"PickupNormal")
		wait
	PickupNormal:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupMarathon:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("Green", 0.3, 3)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupQuake:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("LavaNails", 3)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupRott:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("HSMissiles", 3)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupBlake:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 12)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupDisruptor:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("DisruptorHiFreq", 4)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupRedneck:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("MotoGunAmmo", 10)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupBitterman:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("Q2Flechettes", 5)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupCatacomb:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Catacomb_Waves", 1)
		TNT1 A 0 A_GiveInventory("Catacomb_Zappers", Random(1,2))
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupPainkiller:
		TNT1 A 0 A_JumpIfInventory("Painkiller_Replenish_Power",1,1)
		Goto PickupPainkiller+3
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("Painkiller_FlamerAmmo", 4)
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("Painkiller_FlamerAmmo", 4)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupRTCW:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("RTCW_AlliedAmmo1", 5)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupSarge:
		TNT1 A 0 A_JumpIfInventory("PickupFlashesAreDisabled",1,2)
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_GiveInventory("Clip", 10)
		TNT1 A 0 A_GiveInventory("Q3ChaingunAmmo", 10)
		TNT1 A -1 A_Jump(256,"PickupSoundNormal")
		wait
	PickupSoundNormal:
		TNT1 A 0 ACS_NamedExecuteAlways("SamsaraPickupMessages", 0, 67, 0, 0)
		TNT1 A 1 A_PlaySound("ammo/bulletsmall", CHAN_ITEM)
		stop
	PickupFail:
		AMSM A 1
		fail
	}
}

