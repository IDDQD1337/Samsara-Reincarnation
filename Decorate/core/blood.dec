Actor GenericGoreActor : SamsaraEffectBase
{
	+FORCEXYBILLBOARD
	+MOVEWITHSECTOR
	+NOBLOCKMAP
	-NOGRAVITY
	-SOLID
}

Actor GenericGoreActorFadeOut : GenericGoreActor
{
	States
	{
		FadeGore:
			"####" "#" 2100
			"####" "#" 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraClientDecorate",13) == 1, "FadeGore")
