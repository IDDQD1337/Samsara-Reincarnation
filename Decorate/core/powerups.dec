        goto UseDefault

      UseDefault:
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlight",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightInfinite",1,"UseFail")
        goto UsePlaySound

      UseHeretic:
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightRaven",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightInfiniteRaven",1,"UseFail")
        goto UsePlaySound

      UseHexen1:
        goto UseHeretic

      UseDeusEx:
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightDeusEx1",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightDeusEx2",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightDeusEx3",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightDeusEx4",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightDeusEx5",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerSamsaraFlightInfinite",1,"UseFail")
        goto UsePlaySound

      UsePlaySound:
        TNT1 A 0 A_JumpIfInventory("CorvusClass",1,"UsePlaySoundHeretic")
        TNT1 A 0 A_JumpIfInventory("HexenClass",1,"UsePlaySoundHexen1")
        TNT1 A 0 A_Jump(256,"UsePlaySoundDefault") // for inheriting actors
        goto UsePlaySoundDefault

      UsePlaySoundDefault:
        TNT1 A 0 A_PlaySound("samsaraitemuse",CHAN_ITEM)
        goto UseSuccess

      UsePlaySoundHeretic:
        TNT1 A 0 A_PlaySound("heretic/itemuse",CHAN_ITEM)
        goto UseSuccess

      UsePlaySoundHexen1:
        TNT1 A 0 A_PlaySound("hexen/artiuse",CHAN_ITEM)
        goto UseSuccess

      UseSuccess:
        TNT1 A 0 A_JumpIfInventory("CorvusClass",1,"UseSuccessHeretic")
        TNT1 A 0 A_JumpIfInventory("HexenClass",1,"UseSuccessHexen1")
        TNT1 A 0 A_JumpIfInventory("DukeClass",1,"UseSuccessDuke")
        TNT1 A 0 A_JumpIfInventory("RMRClass",1,"UseSuccessRMR")
        TNT1 A 0 A_JumpIfInventory("WitchavenClass",1,"UseSuccessWitchaven")
        TNT1 A 0 A_JumpIfInventory("DeusExClass",1,"UseSuccessDeusEx")
        TNT1 A 0 A_Jump(256,"UseSuccessDefault") // for inheriting actors
        goto UseSuccessDefault

      UseSuccessDefault:
        TNT1 A 0 A_JumpIf(CallACS("SamsaraDecorate", 49) == 8, "UseSuccessDefaultInfinite")
        TNT1 A 0 A_GiveInventory("PowerSamsaraFlight")
        goto UseFinish

      UseSuccessDefaultInfinite:
        TNT1 A 0 A_GiveInventory("PowerSamsaraFlightInfinite")
        goto UseFinish

      UseSuccessHeretic:
