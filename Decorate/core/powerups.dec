        TNT1 A 0 A_GiveInventory("PowerSamsaraFlightDeusEx3")
        goto UseSuccessDeusExFinish

      UseSuccessDeusExAdvanced:
        TNT1 A 0 A_GiveInventory("PowerSamsaraFlightDeusEx4")
        goto UseSuccessDeusExFinish

      UseSuccessDeusExMaster:
        TNT1 A 0 A_GiveInventory("PowerSamsaraFlightDeusEx5")
        goto UseSuccessDeusExFinish

      UseSuccessDeusExInfinite:
        TNT1 A 0 A_GiveInventory("PowerSamsaraFlightInfinite")
        goto UseSuccessDeusExFinish

      UseSuccessDeusExFinish:
        TNT1 A 0 ACS_NamedExecuteWithResult("DeusEx_Decorate", 26, 200)
