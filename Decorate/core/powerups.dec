actor PortIconDefender : CustomInventory // special handling
{
    Inventory.UseSound "" // netplay hack
    Inventory.Icon "ARTIDFND"
    Inventory.MaxAmount 16
    Inventory.InterHubAmount 16
    Inventory.RestrictedTo "HexenPlayer"
    Tag "Icon of the Defender"
    +INVBAR
    +UNDROPPABLE
    States
    {
      // Parias (Reincarnation/Classic) (Ghost Invulnerability)
      Use:
        TNT1 A 0 A_JumpIfInventory("PowerFakeInvulnerableCleric",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerFakeInvulnerableMage",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("PowerFakeInvulnerableFighter",1,"UseFail")
        TNT1 A 0 A_PlaySound("hexen/artiuse",CHAN_ITEM)
        TNT1 A 0 A_JumpIfInventory("HexenClassMode",3,"UseFighter")
        TNT1 A 0 A_JumpIfInventory("HexenClassMode",2,"UseMage")
      UseCleric:
        TNT1 A 0 A_GiveInventory("PowerFakeInvulnerableCleric")
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraHexenInvulnerabilityEffect", 0)
        stop

      // Daedolon (Reflective Invulnerability)
      UseMage:
        TNT1 A 0 A_GiveInventory("PowerFakeInvulnerableMage")
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraHexenInvulnerabilityEffect", 1)
        stop

      // Baratus (Normal Invulnerability)
      UseFighter:
        TNT1 A 0 A_GiveInventory("PowerFakeInvulnerableFighter")
        stop

      UseFail:
        TNT1 A 0
        fail
    }
}

