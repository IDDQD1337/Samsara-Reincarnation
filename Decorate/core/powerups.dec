actor PortIconDefender : CustomInventory
{
    Inventory.PickupMessage "Icon of the Defender"
    Inventory.PickupSound "hexen/artiget"
    Inventory.PickupFlash "PickupFlash"
    Inventory.UseSound ""
    Inventory.Icon "ARTIDFND"
    Inventory.MaxAmount 16
    inventory.InterHubAmount 16
    Inventory.RestrictedTo "HexenPlayer"
    Tag "Icon of the Defender"
    +FLOATBOB
    +INVBAR
    +UNDROPPABLE
    States
    {
      Spawn:
        DEFN ABCD 3
        loop

      // Parias (Reincarnation/Classic) (Ghost Invulnerability)
      Use:
        TNT1 A 0 A_JumpIfInventory("SamsaraPowerInvulnerableGreyCleric",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("SamsaraPowerInvulnerableGreyMage",1,"UseFail")
        TNT1 A 0 A_JumpIfInventory("SamsaraPowerInvulnerableGreyFighter",1,"UseFail")
        TNT1 A 0 A_PlaySound("hexen/artiuse",CHAN_ITEM)
        TNT1 A 0 A_JumpIfInventory("HexenClassMode",3,"UseFighter")
        TNT1 A 0 A_JumpIfInventory("HexenClassMode",2,"UseMage")
      UseCleric:
        TNT1 A 0 A_GiveInventory("SamsaraPowerInvulnerableGreyCleric")
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraHexenInvulnerabilityEffect", 0)
        stop

      // Daedolon (Reflective Invulnerability)
      UseMage:
        TNT1 A 0 A_GiveInventory("SamsaraPowerInvulnerableGreyMage")
        TNT1 A 0 ACS_NamedExecuteWithResult("SamsaraHexenInvulnerabilityEffect", 1)
        stop

      // Baratus (Normal Invulnerability)
      UseFighter:
        TNT1 A 0 A_GiveInventory("SamsaraPowerInvulnerableGreyFighter")
        stop

      UseFail:
        TNT1 A 0
        fail
    }
}

