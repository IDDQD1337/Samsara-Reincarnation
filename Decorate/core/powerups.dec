actor PortShadowArmor : PowerupGiver
{
	+FLOORCLIP
	+VISIBILITYPULSE
	+INVENTORY.INVBAR
	+INVENTORY.UNDROPPABLE
	RenderStyle Translucent
	Inventory.MaxAmount 2
	inventory.interhubamount 2
	Powerup.Type Shadow
	Inventory.Icon "I_SHDO"
	Inventory.UseSound "strife/itempickup"
