		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)
		TNT1 A 0 A_PlaySound("heretic/itemget", CHAN_ITEM)
        TNT1 A 1 A_GiveInventory("PortRingInvuln")
		stop
	PickupInvulnHexen:
		TNT1 A 0 ACS_NamedExecuteAlways("SamsaraPickupMessages", 0, 54, 0, 0)
