Actor InvulnPowerup : CustomInventory replaces InvulnerabilitySphere
{
	RenderStyle Add
	Alpha 0.95
	+FLOATBOB
    +INVENTORY.PICKUPFLASH
    +INVENTORY.BIGPOWERUP
