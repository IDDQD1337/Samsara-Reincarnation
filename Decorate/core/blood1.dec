actor NashGoreGibBase : GenericGoreActor
{
	Health 1
	Radius 8
	Height 8
	Mass 1
	Scale 0.75
	+MISSILE
}

actor NashGore_Gib1 : NashGoreGibBase
{
	States
	{
	Spawn:
		TNT1 A 0
		GIB1 A 8 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),SXF_TRANSFERTRANSLATION|SXF_NOCHECKPOSITION) //A_SpawnDebris("Switchable_FlyingBlood",1)
		Loop
	Death:
	Crash:
		GIB1 A 2100
		GIB1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraDecorate",31) == 1, "Crash")
		GIB1 AAAAAAAAAA 1 A_FadeOut(0.1)
		Stop
	}
}

