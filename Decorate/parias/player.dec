actor HexenPlayerPainSoundTrigger : Trigger
{
    States
    {
      Pickup:
        TNT1 A 0 A_JumpIfInventory("HexenClassMode",2,"PickupFighter")
        TNT1 A 0 A_JumpIfInventory("HexenClassMode",1,"PickupMage")
      PickupCleric:
        TNT1 A 0 A_Pain
      PickupFinish:
        TNT1 A 0 A_RailWait
        stop

      PickupMage:
        TNT1 A 0 A_PlaySound("Mage/Pain",CHAN_VOICE)
        goto PickupFinish

      PickupFighter:
        TNT1 A 0 A_PlaySound("Fighter/Pain",CHAN_VOICE)
        goto PickupFinish
    }
}

