			RW00 A 0 A_TakeInventory("RTCW_MG42CoolLock")
            RW00 A 0 A_PlaySound("RTCW/UnderwaterFire",CHAN_5)
            RW00 A 17 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
            goto ReadyActual

		FireStart:
