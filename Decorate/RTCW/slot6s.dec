			RW00 B 0 A_JumpIfInventory("RTCW_MG42FireFrame",5,"FireAnim6")
			RW00 B 0 A_JumpIfInventory("RTCW_MG42FireFrame",4,"FireAnim5")
			RW00 B 0 A_JumpIfInventory("RTCW_MG42FireFrame",3,"FireAnim4")
