            RW00 A 0 A_GiveInventory("RTCW_MG42Magazine")
            loop

		Kicking:
			RW00 A 0 A_GunFlash("KickingFoot")
		KickingDelay:
			RW00 A 18 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
			RW00 A 0 A_TakeInventory("RTCW_Kicking",1)
			goto ReadyActual
	}
}

actor SamsaraRTCWMG42AttackHandler : Trigger
{
    States
    {
      // Ammo checking/taking stuff
      Pickup:
        TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",1,"PickupReloadable")
      PickupNonReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("Cell",1,1)
        stop
        TNT1 A 0 A_TakeInventory("Cell",1)
        goto PickupAttack

      PickupReloadable:
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"PickupAttack")
        TNT1 A 0 A_JumpIfInventory("RTCW_MG42Magazine",1,1)
        stop
        TNT1 A 0 A_TakeInventory("RTCW_MG42Magazine",1)
        goto PickupAttack

      // Attack stuff
      PickupAttack:
        TNT1 A 0 A_PlaySound("RTCW/MG42/Fire",CHAN_WEAPON)
        TNT1 A 0 A_JumpIfInventory("SPModeOn",1,"PickupAttackCoop")
        TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"PickupAttackCoop")
      PickupAttackDM:
        TNT1 A 0 A_FireCustomMissile("RTCW_MG42Tracer",CallACS("RTCW_Decorate",1,1)*frandom(-1.0,1.0),false,6,9,0,CallACS("RTCW_Decorate",1,1)*frandom(-1.0,1.0))
        goto PickupFinish

      PickupAttackCoop:
        TNT1 A 0 A_FireCustomMissile("RTCW_MG42TracerCoop",CallACS("RTCW_Decorate",1,1)*frandom(-1.0,1.0),false,6,9,0,CallACS("RTCW_Decorate",1,1)*frandom(-1.0,1.0))
        goto PickupFinish

      // Finishing stuff
      PickupFinish:
        TNT1 A 0 A_GiveInventory("RTCW_FG42Spread")
        TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,2)
        TNT1 A 0 A_GiveInventory("RTCW_MG42Heat",2)
        TNT1 A 0 A_JumpIfInventory("SamsaraRecoilIsDisabled",1,2)
        TNT1 A 0 A_SetPitch(pitch-0.25)
        TNT1 A 0 A_SetAngle(angle+frandom(-0.25,0.25))
        stop
    }
}

actor RTCW_MG42Magazine : Ammo
{
    Inventory.MaxAmount 150
    Tag "Portable MG42 Magazine"
    +IGNORESKILL
}

Actor RTCW_MG42Tracer : RTCW_Tracer
{
	DamageType "RTCWVenomBullet"
	States
	{
		Crash:
		Death:
			TNT1 A 0 A_Explode(CallACS("RTCW_BulletDamage",18),8,!XF_HURTSOURCE,0,8)
			Goto Super::Death
		XDeath:
			TNT1 A 0 A_Explode(CallACS("RTCW_BulletDamage",18),8,!XF_HURTSOURCE,0,8)
			Goto Super::XDeath
	}
}

Actor RTCW_MG42TracerCoop : RTCW_MG42Tracer { +THRUSPECIES Species "Player" }
