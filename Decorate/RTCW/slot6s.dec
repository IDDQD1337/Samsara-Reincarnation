            RW00 B 0 A_JumpIfInventory("RTCW_MG42Magazine",1,"FireStart")
        FireReloadableDryCheck:
            RW00 B 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"ReloadStart")
            RW00 B 0 A_JumpIfInventory("Cell",1,"ReloadStart")
            goto FireDry

        FireDry:
            RW00 A 0 A_ClearReFire
			RW00 A 0 A_TakeInventory("RTCW_MG42CoolLock")
            RW00 A 0 A_PlaySound("RTCW/NoAmmo",CHAN_5)
            RW00 A 17 A_WeaponReady(WRF_NOSWITCH|WRF_NOFIRE)
			RW00 A 0 A_SelectWeapon("RTCW_Luger")
			goto Deselect

