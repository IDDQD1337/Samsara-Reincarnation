	GE00 A 1 A_Raise
	goto SelectDualCheck
	GE30 A 1 A_Raise
	goto SelectDualCheck

  Fire:
	GE00 A 0 A_JumpIfInventory("BondSecondaryToken",1,"AltGotoFire")
	GE00 A 0 A_JumpIfInventory("BondAltToggle", 1, "DualWieldFire")
	GE30 A 0 A_JumpIfInventory("BondDualWieldToken", 1, 1)
	Goto Fire+4
	GE30 A 0 A_JumpIfInventory("BondAlternateFire", 1, "Ready")
	TNT1 A 0 A_JumpIfInventory("SamsaraReloadMode",1,"FireReloadable")
  FireNonReloadable:
	TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,"FireStart")
	TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"FireStart")
	goto FireDry

  FireReloadable:
	TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,"FireStart")
	TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,"FireStart")
	goto FireDry

  FireDry:
	GE30 A 0 A_JumpIfInventory("BondDryFireSpamProtector1", 1, "FireFinish")
	GE30 A 0 A_PlaySound("Goldeneye/DryFire",CHAN_6)
	GE30 A 0 ACS_NamedExecuteAlways("BondDryFireSpamBlocker_Primary")
	GE30 A 0 A_GiveInventory("BondDryFireSpamProtector1", 1)
	Goto FireFinish

  FireStart:
	GE30 A 0 A_GiveInventory("BondFiringToken", 1)
	TNT1 A 0 A_TakeInventory("BondGunAngleFiringSingle")
	GE30 A 0 A_AlertMonsters
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 10, "FireRight5")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 9, "FireRight4")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 8, "FireRight3")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 7, "FireRight2")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 6, "FireRight1")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 5, "FireCenter")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 4, "FireLeft1")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 3, "FireLeft2")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 2, "FireLeft3")
	GE30 A 0 A_JumpIfInventory("BondGunAngle", 1, "FireLeft4")
	Goto FireCenter

