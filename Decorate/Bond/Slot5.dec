Actor Goldeneye_Rocket
{
	Damage (0)
	Projectile
	Height 8
	Radius 6
	Speed 20
	-NOGRAVITY
	+FORCERADIUSDMG
	+EXTREMEDEATH
	Gravity 0.5
	Scale 0.75
	DeathSound "Goldeneye/Explosion"
	States
	{
		Spawn:
			GE00 A 0
			GE00 A 0 A_SpawnItemEx("Goldeneye_RocketSmoke", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
			GE00 A 0 A_JumpIfInventory("BondRocketLifeTime", 0, 2)
			GE00 A 0 A_JumpIfInventory("BondRocketLifeTime", 10, "AdjustPitch")
			GE00 A 0 A_GiveInventory("BondRocketLifeTime", 1)
			GE00 A 1 Bright
			loop
		AdjustPitch:
			GE00 A 0 ThrustThingZ(0, 24, 0, 1)
			GE00 A 0 A_SetGravity(0.01)
			GE00 A 0 A_TakeInventory("BondRocketLifeTime", 11)
			Goto SpawnLoop
		SpawnLoop:
			GE00 A 0 A_SpawnItemEx("Goldeneye_RocketSmoke", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
			GE00 A 1 Bright
			loop
