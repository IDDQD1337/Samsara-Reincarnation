ACTOR Goldeneye_RocketLauncher : Weapon
{
  Weapon.SelectionOrder 5
  Weapon.AmmoType1 "Goldeneye_RocketLauncherMagazine"
  Weapon.AmmoUse1 1
  Weapon.AmmoGive1 0
  Weapon.AmmoType2 "RocketAmmo"
  Weapon.AmmoUse2 0
  Weapon.AmmoGive2 3
  Weapon.SlotNumber 5
  Weapon.BobStyle InverseSmooth
  Weapon.BobSpeed 1.6
  Weapon.BobRangeX 0.3
  Weapon.BobRangeY 0.4  
  Obituary "%o was blown up by %k's rocket launcher."
  Inventory.Pickupmessage "Picked up a Rocket Launcher."
  Inventory.RestrictedTo "JamesBond"
  Tag "Rocket Launcher"
  +WEAPON.AMMO_OPTIONAL
  +WEAPON.EXPLOSIVE
  +WEAPON.NOALERT
  Height 16
  Radius 8
