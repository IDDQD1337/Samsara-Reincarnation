	TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",1,3)
	TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,2)
	GE01 E 1 A_WeaponReady
	goto ReadyNoAmmoCheck
	GE31 E 1 A_WeaponReady
	goto ReadyAmmoCheck
  Right5Reloadable:
	TNT1 A 0 A_JumpIfInventory("SamsaraHasInfiniteAmmo",0,3)
	TNT1 A 0 A_JumpIfInventory("Goldeneye_RocketLauncherMagazine",1,2)
	GE01 E 1 A_WeaponReady(WRF_ALLOWRELOAD)
	goto ReadyAmmoCheck
	GE31 E 1 A_WeaponReady(WRF_ALLOWRELOAD)
	goto ReadyAmmoCheck
