Actor Goldeneye_D5KTracer : Goldeneye_BulletTracer
{
	Damage (12)
	states
	{
		Spawn:
			GE00 A 0
			GE00 A 0 A_SetUserVar("user_damage",12)
			TNT1 A 1
			GE00 A 0 A_ChangeVelocity(velx*2,vely*2,velz*2,CVF_REPLACE)
		SpawnLoop:
			GE00 A 1 Bright
			loop
	}
}

