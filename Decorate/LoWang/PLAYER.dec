  Death.DukePistol:
    TNT1 A 0 A_Jump(218,"Death")
    TNT1 A 0 A_GiveToTarget("DukeDMTaunt1",1)
    goto Death
  Death.DukeExplosive:
    TNT1 A 0 A_JumpIfHealthLower(-5,"DukeGibStart")
    goto Death.DukeDamage
  DukeGibStart:
    SPL1 N 1
    TNT1 A 0 A_UnsetSolid
    TNT1 A 0 A_PlaySound("duke/enemygib")
    TNT1 A 0 A_SpawnDebris("DukeGibs1")
    TNT1 A 0 A_SpawnDebris("DukeGibs2")
    TNT1 A 0 A_SpawnDebris("DukeGibs3")
    TNT1 A 0 A_SpawnDebris("DukeGibs4")
    TNT1 A 0 A_SpawnDebris("DukeGibs5")
    TNT1 A 0 A_SpawnDebris("DukeGibs6")
    TNT1 A 0 A_Jump(96,"XDeath")
    TNT1 A 0 A_GiveToTarget("DukeDMTaunt1",1)
    goto XDeath
  Death.ZorchPistol:
  Death.Zorch:
  Death.ZorchKamikaze:
  Death.PhasingZorch:
  Death.ZorchSpectral:
	TNT1 A 0 A_TakeInventory("MagicCircleCounter", 999)
	TNT1 A 0 A_TakeInventory("MagicCircleSpawner", 999)    
    TNT1 S 0 A_Stop
    TNT1 S 0 A_PlaySoundEx("chex/zorch","soundslot5",0,0)
    SPL1 N 3 A_SpawnItem("ZorchEffectSmall")
    SPL1 N 3 A_SetTranslucent(0.9,0)
    SPL1 N 0 A_PlayerScream
    SPL1 N 3 A_SetTranslucent(0.8,0)
    SPL1 N 0 A_NoBlocking
    SPL1 N 3 A_SetTranslucent(0.7,0)
    SPL1 N 3 A_SetTranslucent(0.6,0)
    SPL1 N 3 A_SetTranslucent(0.5,0)
    SPL1 N 3 A_SetTranslucent(0.4,0)
    SPL1 N 3 A_SetTranslucent(0.3,0)
    SPL1 N 3 A_SetTranslucent(0.2,0)
    SPL1 N 3 A_SetTranslucent(0.1,0)
    ACLO E 1 A_SetTranslucent(1,0)
    ACLO E -1
    stop
  Disintegrate:
	TNT1 A 0 A_TakeInventory("MagicCircleCounter", 999)
	TNT1 A 0 A_TakeInventory("MagicCircleSpawner", 999)    
    TNT1 A 0 A_GiveInventory("EnemyZorched",1)
    TNT1 A 0 A_NoBlocking
    TNT1 A 0 A_ChangeFlag("DONTGIB",1)
    TNT1 A 0 A_SpawnItemEx("LoWangDisintegrate",0,0,5,0,0,0,0,32)
    ACLO E 5 A_PlayerScream
    ACLO E -1
    stop
  }
}

