	SPL1 A 0 A_JumpIf(waterlevel > 2, "Swimming")
    SPL1 AAAA 1 A_JumpIf(momz >> 0, "Jumping")
	SPL1 A 0 A_JumpIf(waterlevel > 2, "Swimming")
	SPL1 BBBB 1 A_JumpIf(momz >> 0, "Jumping")
	SPL1 A 0 A_JumpIf(waterlevel > 2, "Swimming")
	SPL1 CCCC 1 A_JumpIf(momz >> 0, "Jumping")
	SPL1 A 0 A_JumpIf(waterlevel > 2, "Swimming")
	SPL1 DDDD 1 A_JumpIf(momz >> 0, "Jumping")
    Loop
  Jumping:
    SPL5 A 1
    SPL5 AA 1
	SPL5 BB 1
  FlightC:
	SPL5 C 0 A_JumpIf(waterlevel > 2, "Swimming")
	SPL5 C 1
	SPL5 CC 1 A_JumpIf(momz >> 0, "FlightC")
    SPL5 D 3 A_JumpIf(momz >> 0, "FlightC")
    Goto Spawn
  Pain:
	SPL1 A 0 A_JumpIf(waterlevel > 2, "Swimming")
    SPL1 A 4
    SPL1 A 4 A_Pain
    Goto Spawn
  Missile:
    SPL1 A 0 A_JumpIf(waterlevel > 2, "Swimming")
	TNT1 A 0 A_JumpIFInventory("SWKatanaUsed", 1, "SwordAttack")
	TNT1 A 0 A_JumpIfInventory("SWFistUsed", 1, "FistAttack")
    SPL1 EE 3
    Goto Spawn
  SwordAttack:
	SPL1 GHI 3
	Goto Spawn
  FistAttack:
    SPL1 JK 3
    Goto Spawn
  Melee:
	SPL1 A 0 A_JumpIf(waterlevel > 2, "Swimming")
    SPL1 F 6 BRIGHT
    SPL1 F 6
    Goto Spawn
  Swimming:
	SPL2 AAAA 1 A_JumpIf(waterlevel < 3, "See")
	SPL2 BBBB 1 A_JumpIf(waterlevel < 3, "See")
	SPL2 CCCC 1 A_JumpIf(waterlevel < 3, "See")
	SPL2 DDDD 1 A_JumpIf(waterlevel < 3, "See")
	Loop
  Pain.LifeLeech:
    TNT1 A 0 A_GiveToTarget("SerpentHealth", random(1,2))
    TNT1 A 0 A_GiveToTarget("HealthBonus",1)
	Goto Pain
  Pain.SWStickyCoop:	
    SPL1 A 4
    SPL1 A 4 A_PlaySound("SWPAINSTICKY",CHAN_VOICE)
	Goto Spawn
  Death:
	TNT1 A 0 A_TakeInventory("MagicCircleCounter", 999)
	TNT1 A 0 A_TakeInventory("MagicCircleSpawner", 999)	  
    TNT1 A 0 A_SpawnItem("UniversalBloodyHellGoreDeathEffect")
	SPL1 N 4
	SPL1 N 4 A_PlayerScream	
    SPL1 O 8 A_NoBlocking 
    SPL1 P 8
    SPL1 Q 8
    SPL1 RSTU 8
    SPL1 V -1
    Stop
  XDeath:
	TNT1 A 0 A_TakeInventory("MagicCircleCounter", 999)
	TNT1 A 0 A_TakeInventory("MagicCircleSpawner", 999)
    TNT1 A 0 A_SpawnItem("UniversalBloodyHellGoreXDeathEffect")
	TNT1 A 0 A_UnsetSolid
	TNT1 A 0 A_SpawnItemEx("SWHumanGibSpawner_LoWang")			
	TNT1 A 0 A_SpawnItemEx("LoWangHead",random(-12,12),random(-12,12),random(0,40),random(-5,5),random(-5,5),random(4,10),0,SXF_CLIENTSIDE)	
    TNT1 A 0 A_PlaySound("sw/gib_start")
    ACLO E 5 A_NoBlocking
    ACLO E -1
    Stop
  Death.PhosFireCoop:
  Death.HexGreenFire:
  Death.QuakeFire:
  Death.FireExplosive:
  Burn:
	TNT1 A 0 A_TakeInventory("MagicCircleCounter", 999)
	TNT1 A 0 A_TakeInventory("MagicCircleSpawner", 999)  
    TNT1 A 0 A_Fall
    ACLO E 0 A_SpawnItem("BurnDeathWang")
    ACLO E 5 A_Playsound("SPKILL0")	
    ACLO E -1
    stop
  Death.DukeIce:
    TNT1 A 0 A_Jump(218,"Ice")
    TNT1 A 0 A_GiveToTarget("DukeDMTauntDuke",1)
    goto Ice
  Ice:
  Death.HexenIce:
  Death.Ice:
	TNT1 A 0 A_TakeInventory("MagicCircleCounter", 999)
	TNT1 A 0 A_TakeInventory("MagicCircleSpawner", 999)  
    SPL1 N 70 A_NoBlocking
    SPL1 N 0 A_SetTranslucent(0,0)
    TNT1 AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_SpawnItemEx("IceChunk2",random(-1,1),random(-1,1),random(8,48),random(-2,2),random(-2,2),random(1,6))
    ACLO E 1 A_PlayWeaponSound("misc/icebreak")
	ACLO E -1
    Stop	
