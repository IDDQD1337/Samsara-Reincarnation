actor DukeDevastatorRocket
{
	Radius 3
	Height 2
	Speed 38
	Damage (0)
	Projectile
	Obituary "$SAMSARA_DUKE_OB_SLOT7"
	Scale 0.2
	Decal DoomImpScorch
	DamageType "DukeExplosive"
	+FORCERADIUSDMG
	States
	{
		Spawn:
			RPGX A 1 BRIGHT
			loop

		XDeath:
		Crash:
			TNT1 A 0 A_Explode(38, 90, 0)

		Death:
			TNT1 A 0 A_Explode(38, 90)
